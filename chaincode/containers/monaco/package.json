{
  "name": "@container/monaco",
  "version": "0.1.3",
  "description": "Monaco chaincode container",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": [
    "./src/publicpath.ts"
  ],
  "scripts": {
    "build": "tsc && tsc --outDir lib --m esnext && npm run webpack && npm run tslint",
    "tsc": "tsc",
    "webpack": "webpack --config webpack.config.js",
    "tslint": "tslint 'src/**/*.ts'",
    "start": "prague-dev-server --config webpack.config.js --package package.json"
  },
  "author": "Microsoft",
  "dependencies": {
    "@chaincode/monaco": "^0.3.5291",
    "@prague/container-definitions": "^0.2.5295",
    "@prague/runtime": "^0.2.5295"
  },
  "devDependencies": {
    "@prague/prague-dev-server": "^3.2.3",
    "css-loader": "^1.0.0",
    "html-loader": "^0.5.5",
    "loader-utils": "^1.1.0",
    "monaco-editor-webpack-plugin": "^1.5.4",
    "node-sass": "^4.9.3",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.0",
    "ts-loader": "^4.5.0",
    "tslint": "^5.11.0",
    "typescript": "^3.2.2",
    "url-loader": "^1.1.1",
    "webpack": "^4.7.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14"
  },
  "prague": {
    "browser": {
      "entrypoint": "monacocontainer",
      "bundle": [
        "dist/main.bundle.js"
      ]
    }
  }
}
