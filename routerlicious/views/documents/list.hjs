{{<layout}}
    {{$styles}}
        <link href="{{#hfurl}}/public/stylesheets/dist/style.css{{/hfurl}}" rel="stylesheet" />
    {{/styles}}

    {{$content}}
        <div class="container">
            <h1>Templates</h1>
            <ul class="list-unstyled">
                {{#templates}}
                <li><a href="/sharedText/{{documentId}}?template={{full}}">{{name}}</a></li>
                {{/templates}}
            </ul>
        </div>
    {{/content}}
{{/layout}}