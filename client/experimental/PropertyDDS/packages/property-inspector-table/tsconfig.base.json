{
  "compilerOptions": {
    "strictBindCallApply": true,
    "noImplicitAny": false,
    "target": "es6",
    "lib": ["es2018", "dom"],
    "sourceMap": true,
    "strict": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "noUnusedParameters": false,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "declarationMap": true
  }
}
