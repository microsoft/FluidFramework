{
  "name": "@fluid-experimental/property-binder",
  "version": "4.3.1",
  "description": "Data Binder for Fluid PropertDDS",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "build": "webpack --config webpack.prod.js",
    "postbuild": "npm run gen:tscdef",
    "doc": "cross-var appfw-typedoc $npm_package_name",
    "gen:tscdef": "tsc -p tsconfig.json --outDir dist -d",
    "lint": "eslint .",
    "precit": "bash -c \"npm run test -- --reporters dots; echo '----------Linting----------'; npm run lint; echo '----------Docs----------'; npm run doc\"",
    "prepublishOnly": "npm run test:dist",
    "test": "karma start  --single-run",
    "test:performance": "karma start  karma.performance.conf.js --single-run",
    "test:performance:dev": "karma start  karma.performance.conf.js --browsers ChromeDebug",
    "pretest:coverage": "rimraf coverage/",
    "test:coverage": "karma start --runCoverage --single-run",
    "posttest:coverage": "(xdg-open _build/coverage/html/index.html > /dev/null 2>/dev/null) || (open _build/coverage/html/index.html > /dev/null 2>/dev/null) || (start _build/coverage/html/index.html) || true",
    "test:dev": "karma start --browsers Chrome",
    "test:dist": "mocha --exit test/release/**/*.spec.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.13.7",
    "@fluid-experimental/property-changeset": "0.1.0",
    "@fluid-experimental/property-properties": "0.1.0",
    "underscore": "^1.9.1"
  },
  "peerDependencies": {},
  "devDependencies": {
    "@fluidframework/build-common": "^0.20.0-0",
    "rimraf": "^2.6.2",
    "ts-loader": "^6.1.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
    "typedoc": "^0.12.0",
    "typescript": "~4.1.3",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "underscore": "^1.9.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.8.0",
    "webpack-glsl-loader": "^1.0.1",
    "webpack-karma-die-hard": "^1.0.4",
    "webpack-license-plugin": "^4.0.1",
    "webpack-merge": "^4.1.4",
    "clean-webpack-plugin": "^3.0.0",
    "async": "^2.6.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-istanbul": "^5.1.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-jsx": "^2.0.0",
    "body-parser": "^1.18.3",
    "chai": "^4.2.0",
    "eslint": "~7.18.0",
    "jsdoc": "3.5.5",
    "lighthouse": "^5.2.0",
    "source-map-loader": "^0.2.4",
    "source-map-support": "^0.5.16",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.2.0",
    "babel-plugin-polyfill-corejs2": "^0.1.10",
    "babel-plugin-polyfill-corejs3": "^0.1.7",
    "babel-plugin-polyfill-regenerator": "^0.1.6",
    "@types/underscore": "^1.9.1",
    "@types/jest": "22.2.3",
    "jest": "^26.6.3",
    "jest-junit": "^10.0.0",
    "ts-jest": "^26.4.4"
  },
  "files": [
    "dist",
    "index.js",
    "index.d.ts"
  ]
}
