{
	"name": "@fluid-experimental/tree",
	"version": "0.33.0",
	"description": "Distributed tree",
	"homepage": "https://fluidframework.com",
	"repository": "https://github.com/microsoft/FluidFramework",
	"license": "MIT",
	"author": "Microsoft",
	"sideEffects": false,
	"main": "dist/index.js",
	"module": "lib/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "concurrently npm:build:compile npm:lint",
		"build:compile": "concurrently npm:tsc npm:build:esnext",
		"build:docs": "api-extractor run --local",
		"build:esnext": "tsc --project ./tsconfig.esnext.json",
		"build:full": "npm run build",
		"build:full:compile": "npm run build:compile",
		"clean": "rimraf dist lib *.tsbuildinfo *.build.log",
		"eslint": "eslint src",
		"eslint:fix": "eslint src --fix",
		"lint": "npm run prettier && npm run eslint",
		"lint:fix": "npm run prettier:fix && npm run eslint:fix",
		"prettier": "prettier --check \"**/*.{js,json,jsx,md,ts,tsx,yml,yaml}\"",
		"prettier:fix": "prettier --write \"**/*.{js,json,jsx,md,ts,tsx,yml,yaml}\"",
		"test": "npm run test:mocha",
		"test:mocha": "nyc --report-dir='./coverage' mocha \"dist/**/*.tests.js\"",
		"tsc": "tsc"
	},
	"dependencies": {
		"@fluidframework/common-definitions": "^0.19.1",
		"@fluidframework/common-utils": "^0.27.0-0",
		"@fluidframework/container-definitions": "0.33.0",
		"@fluidframework/core-interfaces": "0.33.0",
		"@fluidframework/datastore-definitions": "0.33.0",
		"@fluidframework/protocol-definitions": "^0.1018.0-0",
		"@fluidframework/shared-object-base": "0.33.0",
		"@fluidframework/telemetry-utils": "0.33.0",
		"buffer": "^6.0.2",
		"immutable": "^4.0.0-rc.12",
		"sorted-btree": "^1.4.0",
		"uuid": "^8.3.1"
	},
	"devDependencies": {
		"@fluidframework/build-common": "^0.19.2",
		"@fluidframework/eslint-config-fluid": "^0.21.0",
		"@fluidframework/test-runtime-utils": "0.33.0",
		"@microsoft/api-extractor": "^7.7.2",
		"@typescript-eslint/eslint-plugin": "~4.2.0",
		"@typescript-eslint/parser": "~4.2.0",
		"chai": "^4.2.0",
		"concurrently": "^5.2.0",
		"easy-table": "^1.1.1",
		"env-cmd": "^10.1.0",
    "eslint": "~7.9.0",
    "eslint-plugin-eslint-comments": "~3.2.0",
    "eslint-plugin-import": "~2.22.0",
    "eslint-plugin-no-null": "~1.0.2",
    "eslint-plugin-prefer-arrow": "~1.2.2",
    "eslint-plugin-react": "~7.21.2",
    "eslint-plugin-unicorn": "~22.0.0",
		"mocha": "^8.1.1",
		"nyc": "^15.0.0",
		"prettier": "^2.2.1",
		"rimraf": "^2.6.2",
		"typescript": "~3.7.4"
	}
}
