{
  "name": "@fluidframework/bundle-size-tools",
  "version": "0.4.7000",
  "description": "Utility for analyzing bundle size regressions",
  "homepage": "https://fluidframework.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/FluidFramework.git",
    "directory": "build-tools/package/bundle-size-tools"
  },
  "license": "MIT",
  "author": "Microsoft and contributors",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build:genver && concurrently npm:build:compile npm:lint && npm run build:docs",
    "build:compile": "concurrently npm:tsc npm:build:copy",
    "build:copy": "copyfiles -u 1 \"src/**/*.fsl\" dist",
    "build:diagrams": "jssm-viz -i \"./src/machines/*.fsl\"",
    "build:docs": "api-extractor run --local",
    "build:full": "npm run build",
    "build:full:compile": "npm run build:compile",
    "build:genver": "gen-version",
    "ci:build:docs": "api-extractor run",
    "clean": "rimraf dist *.tsbuildinfo",
    "eslint": "eslint src",
    "eslint:fix": "eslint src --fix",
    "lint": "npm run eslint",
    "lint:fix": "npm run eslint:fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc": "tsc -b"
  },
  "dependencies": {
    "azure-devops-node-api": "^11.2.0",
    "jszip": "^3.10.1",
    "msgpack-lite": "^0.1.26",
    "pako": "^2.0.4",
    "typescript": "~4.5.5",
    "webpack": "^5.74.0"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.22.2",
    "@types/jszip": "^3.4.1",
    "@types/msgpack-lite": "^0.1.8",
    "@types/node": "^14.18.0",
    "@types/pako": "^2.0.0",
    "rimraf": "^2.6.2"
  }
}
