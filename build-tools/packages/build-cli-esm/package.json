{
	"name": "build-cli-esm",
	"version": "0.4.24",
	"description": "Fluid Framework build tools (ESM)",
	"keywords": ["oclif"],
	"homepage": "https://github.com/packages/build-cli-esm",
	"bugs": "https://github.com/packages/build-cli-esm/issues",
	"repository": "packages/build-cli-esm",
	"license": "MIT",
	"author": "Microsoft",
	"type": "module",
	"exports": "./lib/index.js",
	"types": "dist/index.d.ts",
	"bin": {
		"flub": "./bin/run.js"
	},
	"files": ["/bin", "/dist", "/oclif.manifest.json"],
	"scripts": {
		"build": "shx rm -rf dist && tsc -b",
		"lint": "eslint . --ext .ts",
		"prepack": "npm run build && oclif manifest && oclif readme",
		"postpack": "shx rm -f oclif.manifest.json",
		"prepare": "npm run build",
		"test": "mocha --forbid-only \"test/**/*.test.ts\"",
		"posttest": "npm run lint",
		"version": "oclif readme && git add README.md"
	},
	"oclif": {
		"bin": "flub",
		"commands": "./dist/commands",
		"dirname": "flub",
		"plugins": ["@oclif/plugin-help", "@oclif/plugin-plugins"],
		"topicSeparator": " ",
		"topics": {
			"hello": {
				"description": "Say hello to the world and others"
			}
		}
	},
	"dependencies": {
		"@oclif/core": "^3.26.0",
		"@oclif/plugin-autocomplete": "^3.0.13",
		"@oclif/plugin-commands": "^3.2.2",
		"@oclif/plugin-not-found": "^3.1.1",
		"@oclif/plugin-help": "^6.0.20"
	},
	"devDependencies": {
		"@oclif/prettier-config": "^0.2.1",
		"@oclif/test": "~2.3.33",
		"@types/chai": "^4.3.5",
		"@types/mocha": "^10.0.6",
		"@types/node": "^18.18.6",
		"chai": "^4.3.7",
		"eslint": "~8.57.0",
		"eslint-config-oclif": "^5.1.1",
		"eslint-config-oclif-typescript": "^3.1.3",
		"eslint-config-prettier": "~9.1.0",
		"mocha": "^10.2.0",
		"oclif": "^4.7.6",
		"shx": "^0.3.4",
		"ts-node": "^10.9.2",
		"typescript": "~5.1.6"
	},
	"engines": {
		"node": ">=18.0.0"
	}
}
