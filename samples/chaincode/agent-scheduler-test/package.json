{
  "name": "@chaincode/agent-scheduler-test",
  "version": "0.4.14",
  "description": "Chaincode wrapping agent-scheduler component",
  "author": "Microsoft",
  "repository": "microsoft/FluidFramework",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run tsc && npm run tslint && npm run pack:dev",
    "build:compile": "npm run build",
    "build:esnext": "tsc --project ./tsconfig.esnext.json",
    "build:full": "npm run build && npm run tsc",
    "build:full:compile": "npm run build:compile",
    "clean": "rimraf dist lib *.tsbuildinfo package-deps.json *.build.log",
    "tsc": "tsc",
    "tslint": "tslint --project tsconfig.json --format verbose",
    "pack": "webpack --env=\"production\"",
    "pack:dev": "webpack",
    "start": "node ../../../common/tools/node_modules/@prague/prague-dev-server/bin/prague-dev-server.js --config webpack.config.js --live --package package.json",
    "start:local": "node ../../../common/tools/node_modules/@prague/prague-dev-server/bin/prague-dev-server.js --config webpack.config.js --package package.json",
    "verdaccio": "npm publish --registry https://packages.wu2.prague.office-int.com",
    "deploy": "npm run build && npm version patch && npm run verdaccio"
  },
  "prague": {
    "browser": {
      "entrypoint": "main",
      "bundle": [
        "dist/main.bundle.js"
      ]
    }
  },
  "dependencies": {
    "@prague/app-component": "^0.4.7702",
    "@prague/container-definitions": "^0.4.7702",
    "@prague/container-runtime": "^0.4.7702",
    "@prague/runtime-definitions": "^0.4.7702",
    "@chaincode/agent-scheduler": "^0.4.7702",
    "debug": "^4.1.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@microsoft/fluid-build-common": "^0.4.7702",
    "@types/node": "^10.12.12",
    "rimraf": "^2.6.2",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^4.5.0",
    "tslint": "^5.11.0",
    "typescript": "^3.4.0",
    "webpack": "^4.8.2",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.2"
  },
  "license": "MIT"
}
