{
  "name": "@fluid-example/key-value-cache",
  "version": "0.11.1",
  "description": "Key-value cache that runs in the node.js service",
  "repository": "microsoft/FluidFramework",
  "license": "MIT",
  "author": "Microsoft",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "webpack --env=\"production\" && npm run tsc",
    "deploy": "npm run build && npm version patch && npm run verdaccio && npm run post-deploy",
    "dev": "webpack --env=\"development\"",
    "start": "webpack-dev-server --config webpack.config.js --package package.json",
    "start:live": "webpack-dev-server --config webpack.config.js --package package.json --env.mode live",
    "start:localhost": "webpack-dev-server --config webpack.config.js --package package.json --env.mode localhost",
    "tsc": "tsc",
    "verdaccio": "npm publish --registry https://packages.wu2.prague.office-int.com",
    "webpack": "npm run build",
    "webpack:dev": "npm run dev"
  },
  "dependencies": {
    "@microsoft/fluid-component-core-interfaces": "0.11.15658",
    "@microsoft/fluid-component-runtime": "0.11.15658",
    "@microsoft/fluid-container-definitions": "0.11.15658",
    "@microsoft/fluid-container-runtime": "0.11.15658",
    "@microsoft/fluid-map": "0.11.15658",
    "@microsoft/fluid-runtime-definitions": "0.11.15658",
    "@microsoft/fluid-shared-object-base": "0.11.15658",
    "debug": "^4.1.1"
  },
  "devDependencies": {
    "@microsoft/fluid-build-common": "0.11.15658",
    "@microsoft/fluid-webpack-component-loader": "0.11.15658",
    "@types/node": "^10.14.6",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^4.5.0",
    "typescript": "~3.4.5",
    "webpack": "4.16.5",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "4.1.4"
  },
  "fluid": {
    "browser": {
      "umd": {
        "files": [
          "dist/main.bundle.js"
        ],
        "library": "main"
      }
    }
  }
}
