{
  "name": "@chaincode/key-value-cache",
  "description": "Key-value cache that runs in the node.js service",
  "version": "0.9.2",
  "author": "Microsoft",
  "repository": "microsoft/FluidFramework",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "webpack --env=\"production\" && npm run tsc",
    "dev": "webpack --env=\"development\"",
    "start": "prague-dev-server --config webpack.config.js --package package.json",
    "verdaccio": "npm publish --registry https://packages.wu2.prague.office-int.com",
    "deploy": "npm run build && npm version patch && npm run verdaccio && npm run post-deploy",
    "post-deploy": "getUrl -p",
    "tsc": "tsc",
    "webpack": "npm run build",
    "webpack:dev": "npm run dev"
  },
  "devDependencies": {
    "@microsoft/fluid-build-common": "0.9.12046",
    "@prague/prague-dev-server": "~3.9.0",
    "@prague/url-generator": "^1.0.4",
    "@types/node": "^10.14.6",
    "ts-loader": "^4.5.0",
    "typescript": "~3.4.5",
    "webpack": "4.16.5",
    "webpack-cli": "3.1.0",
    "webpack-merge": "4.1.4"
  },
  "fluid": {
    "browser": {
      "umd": {
        "files": [
          "dist/main.bundle.js"
        ],
        "library": "main"
      }
    }
  },
  "dependencies": {
    "@prague/component-core-interfaces": "0.9.12046",
    "@prague/component-runtime": "0.9.12046",
    "@prague/container-definitions": "0.9.12046",
    "@prague/container-runtime": "0.9.12046",
    "@prague/map": "0.9.12046",
    "@prague/runtime-definitions": "0.9.12046",
    "@prague/shared-object-common": "0.9.12046",
    "debug": "^4.1.1"
  },
  "license": "MIT"
}
