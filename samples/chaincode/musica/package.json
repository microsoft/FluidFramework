    "rc-slider": "^8.6.9",
    "rc-tooltip": "^3.7.3",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-piano": "^3.1.2",
    "react-widgets": "^4.4.11",
    "soundfont-player": "^0.11.0",
    "url-loader": "^1.1.2",
    "yarn": "^1.15.2"
  },
  "license": "MIT"
}
