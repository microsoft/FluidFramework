{
  "name": "@fluid-example/image-gallery",
  "description": "Image Gallery with pretty layout",
  "version": "0.10.0",
  "author": "Microsoft",
  "repository": "microsoft/FluidFramework",
  "main": "dist/main.bundle.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "webpack --env.production",
    "dev": "webpack --env.development",
    "start": "webpack-dev-server --config webpack.config.js --package package.json",
    "start:local": "webpack-dev-server --config webpack.config.js --package package.json --env.local",
    "start:localhost": "webpack-dev-server --config webpack.config.js --package package.json --env.mode localhost",
    "verdaccio": "npm publish --registry https://packages.wu2.prague.office-int.com",
    "deploy": "npm run build && npm version patch && npm run verdaccio && npm run post-deploy",
    "post-deploy": "node getRoute.js",
    "tsc": "tsc",
    "webpack": "npm run build",
    "webpack:dev": "npm run dev"
  },
  "devDependencies": {
    "@microsoft/fluid-webpack-component-loader": "^0.10.0",
    "@types/node": "10.11.5",
    "@types/react-dom": "^16.0.11",
    "@types/react-image-gallery": "^0.8.4",
    "css-loader": "^2.1.1",
    "ts-loader": "4.4.2",
    "typescript": "3.0.1",
    "typings-for-css-modules-loader": "^1.7.0",
    "webpack": "4.16.5",
    "webpack-dev-server": "^3.8.0",
    "webpack-cli": "3.1.0",
    "webpack-merge": "4.1.4"
  },
  "dependencies": {
    "@microsoft/fluid-aqueduct": "^0.10.0",
    "@microsoft/fluid-component-core-interfaces": "^0.10.0",
    "@microsoft/fluid-container-definitions": "^0.10.0",
    "@microsoft/fluid-map": "^0.10.0",
    "@microsoft/fluid-runtime-definitions": "^0.10.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-image-gallery": "^0.9.1",
    "style-loader": "^0.23.1"
  },
  "fluid": {
    "browser": {
      "umd": {
        "files": [
          "dist/main.bundle.js"
        ],
        "library": "main"
      }
    }
  },
  "license": "MIT"
}
