{
    "extends": [
        "@microsoft/fluid-build-common/tslint.json",
        "tslint-config-prettier"
    ],
    "rules": {
        "completed-docs": false,
        "member-ordering": false,
        "no-console": false,
        "no-unsafe-any": false,
        "no-unnecessary-class": [
            "allow-empty-class",
            "allow-static-only"
        ],
        "no-import-side-effect": [true, {"ignore-module": "(\\.html|\\.css)$"}],
        "function-name": false
    }
}
