{
  "name": "@chaincode/tournament",
  "description": "Chaincode starter project",
  "version": "0.3.0",
  "author": "Microsoft",
  "repository": "microsoft/prague",
  "main": "dist/main.bundle.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "webpack --env=\"production\"",
    "dev": "webpack --env=\"development\"",
    "start": "prague-dev-server --config webpack.config.js --live --package package.json",
    "start:local": "prague-dev-server --config webpack.config.js --package package.json",
    "verdaccio": "npm publish --registry https://packages.wu2.prague.office-int.com",
    "deploy": "npm run build && npm version patch && npm run verdaccio && npm run post-deploy",
    "post-deploy": "node getRoute.js",
    "tsc": "tsc"
  },
  "devDependencies": {
    "@prague/prague-dev-server": "^3.2.5",
    "@types/node": "10.11.5",
    "@types/react-dom": "^16.0.11",
    "css-loader": "^2.1.1",
    "style-loader": "^0.23.1",
    "ts-loader": "4.4.2",
    "typescript": "3.0.1",
    "webpack": "4.16.5",
    "webpack-cli": "3.1.0",
    "webpack-merge": "4.1.4"
  },
  "prague": {
    "browser": {
      "entrypoint": "main",
      "bundle": [
        "dist/main.bundle.js"
      ]
    }
  },
  "dependencies": {
    "@chaincode/schedule": "0.0.3",
    "@prague/app-component": "^0.3.0",
    "@prague/container-definitions": "^0.3.0",
    "@prague/map": "^0.3.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3"
  }
}
