{
  "name": "@fluid-example/github-comment",
  "version": "0.12.0",
  "description": "A GitHub-style textarea that extends the CollaborativeTextAreaNoReact component",
  "repository": "microsoft/FluidFramework",
  "license": "MIT",
  "author": "Microsoft",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "concurrently npm:build:compile npm:tslint",
    "build:compile": "concurrently npm:tsc npm:build:esnext",
    "build:compile:min": "npm run build:compile",
    "build:esnext": "tsc --project ./tsconfig.esnext.json",
    "build:full": "concurrently npm:build npm:webpack",
    "build:full:compile": "concurrently npm:build:compile npm:webpack",
    "clean": "rimraf dist lib *.tsbuildinfo *.build.log",
    "deploy": "npm run build && npm version patch && npm run verdaccio && npm run post-deploy",
    "start": "webpack-dev-server --config webpack.config.js --package package.json",
    "start:live": "webpack-dev-server --config webpack.config.js --package package.json --env.mode live",
    "start:localhost": "webpack-dev-server --config webpack.config.js --package package.json --env.mode localhost",
    "tsc": "tsc",
    "tslint": "tslint --project tsconfig.json --format verbose",
    "verdaccio": "npm publish --registry https://packages.wu2.prague.office-int.com",
    "webpack": "webpack --env.production",
    "webpack:dev": "webpack --env.development"
  },
  "dependencies": {
    "@fluid-example/textarea-noreact": "^0.12.0",
    "@microsoft/fluid-aqueduct": "^0.12.0",
    "@microsoft/fluid-component-core-interfaces": "^0.12.0",
    "@microsoft/fluid-component-runtime": "^0.12.0",
    "@microsoft/fluid-container-definitions": "^0.12.0",
    "@microsoft/fluid-container-runtime": "^0.12.0",
    "@microsoft/fluid-core-utils": "^0.12.0",
    "@microsoft/fluid-framework-interfaces": "^0.12.0",
    "@microsoft/fluid-map": "^0.12.0",
    "@microsoft/fluid-merge-tree": "^0.12.0",
    "@microsoft/fluid-protocol-definitions": "^0.12.0",
    "@microsoft/fluid-runtime-definitions": "^0.12.0",
    "@microsoft/fluid-sequence": "^0.12.0",
    "@microsoft/fluid-shared-object-base": "^0.12.0",
    "markdown-it": "^9.1.0"
  },
  "devDependencies": {
    "@microsoft/fluid-build-common": "^0.12.0",
    "@microsoft/fluid-webpack-component-loader": "^0.12.0",
    "@types/node": "^10.14.6",
    "concurrently": "^4.1.0",
    "css-loader": "^1.0.0",
    "html-loader": "^0.5.5",
    "rimraf": "^2.6.2",
    "style-loader": "^1.0.0",
    "ts-loader": "^6.1.2",
    "tslint": "^5.20.0",
    "tslint-microsoft-contrib": "^6.0.0",
    "typescript": "~3.4.5",
    "url-loader": "^2.1.0",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.1.4"
  },
  "fluid": {
    "browser": {
      "umd": {
        "files": [
          "dist/main.bundle.js"
        ],
        "library": "main"
      }
    }
  }
}
