  },
  "devDependencies": {
    "concurrently": "^3.1.0",
    "cpx": "^1.5.0",
    "rimraf": "^2.5.4",
    "browser-sync": "^2.18.5",
    "typescript": "^2.1.4",
    "@types/office-js": "^0.0.37",
    "@types/jquery": "^2.0.39"
  },
