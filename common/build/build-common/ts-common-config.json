{
	"exclude": ["dist", "node_modules"],
	"compilerOptions": {
		"target": "ES2020",
		"declaration": true,
		"declarationMap": true,
		"esModuleInterop": true,
		"module": "commonjs",
		"moduleResolution": "node",
		"noUnusedLocals": true,
		"sourceMap": true,
		"inlineSources": true,
		"noImplicitAny": false,
		"incremental": true,
		"types": [],
		"strict": true,
		"pretty": true,
		"lib": ["ES2020", "DOM", "DOM.Iterable"]
	}
}
