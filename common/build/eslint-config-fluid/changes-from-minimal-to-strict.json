        "browser": true,
        "es6": true,
        "node": true
    },
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "ignorePatterns": [
        "*.d.ts"
    ],
    "parser": "/workspaces/FluidFramework/node_modules/@typescript-eslint/parser/dist/index.js",
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "project": [
            "./tsconfig.json",
            "./src/test/tsconfig.json"
        ],
        "sourceType": "module"
    },
    "plugins": [
        "promise",
        "@typescript-eslint",
        "@rushstack/security",
        "@rushstack",
        "eslint-comments",
        "import",
        "unicorn",
        "react"
