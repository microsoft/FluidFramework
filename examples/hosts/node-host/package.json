{
  "name": "@fluid-internal/node-host",
  "version": "0.29.0",
  "private": true,
  "description": "Fluid container host for Node.js environment",
  "homepage": "https://fluidframework.com",
  "repository": "microsoft/FluidFramework",
  "license": "MIT",
  "author": "Microsoft",
  "main": "dist/index.js",
  "scripts": {
    "build": "concurrently npm:build:compile npm:lint",
    "build:compile": "npm run tsc",
    "build:full": "npm run build",
    "build:full:compile": "npm run build:compile",
    "clean": "rimraf dist *.tsbuildinfo *.build.log",
    "eslint": "eslint --format stylish src",
    "eslint:fix": "eslint --ext=ts,tsx --format stylish src --fix",
    "lint": "npm run eslint",
    "lint:fix": "npm run eslint:fix",
    "start": "node dist/index.js",
    "start:debug": "node --inspect=0.0.0.0:5858 dist/index.js",
    "tsc": "tsc"
  },
  "dependencies": {
    "@fluid-example/key-value-cache": "^0.29.0",
    "@fluidframework/container-definitions": "^0.29.0",
    "@fluidframework/container-loader": "^0.29.0",
    "@fluidframework/core-interfaces": "^0.29.0",
    "@fluidframework/driver-definitions": "^0.29.0",
    "@fluidframework/protocol-definitions": "^0.1014.0",
    "@fluidframework/routerlicious-driver": "^0.29.0",
    "@fluidframework/routerlicious-host": "^0.29.0",
    "@fluidframework/test-runtime-utils": "^0.29.0",
    "jsonwebtoken": "^8.4.0",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@fluidframework/build-common": "^0.19.2",
    "@fluidframework/eslint-config-fluid": "^0.21.0-0",
    "@typescript-eslint/eslint-plugin": "~4.2.0",
    "@typescript-eslint/parser": "~4.2.0",
    "concurrently": "^5.2.0",
    "eslint": "~7.9.0",
    "eslint-plugin-eslint-comments": "~3.2.0",
    "eslint-plugin-import": "~2.22.0",
    "eslint-plugin-no-null": "~1.0.2",
    "eslint-plugin-prefer-arrow": "~1.2.2",
    "eslint-plugin-react": "~7.21.2",
    "eslint-plugin-unicorn": "~22.0.0",
    "rimraf": "^2.6.2",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^6.1.2",
    "typescript": "~3.7.4"
  }
}
