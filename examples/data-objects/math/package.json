{
  "name": "@fluid-example/math",
  "version": "0.32.0",
  "description": "Chaincode component that provides math.",
  "homepage": "https://fluidframework.com",
  "repository": "https://github.com/microsoft/FluidFramework",
  "license": "MIT",
  "author": "Microsoft",
  "sideEffects": false,
  "main": "dist/mathComponent.js",
  "module": "lib/mathComponent.js",
  "types": "dist/mathComponent.d.ts",
  "scripts": {
    "build": "concurrently \"yarn run build:compile\" \"yarn run lint\"",
    "build:commonjs": "yarn run tsc && yarn run build:test",
    "build:compile": "concurrently \"yarn run build:commonjs\" \"yarn run build:esnext\" \"yarn run build:copy\"",
    "build:copy": "copyfiles -u 1 \"src/**/*.css\" dist/ && copyfiles -u 1 \"src/**/*.css\" lib/",
    "build:esnext": "tsc --project ./tsconfig.esnext.json",
    "build:full": "concurrently \"yarn run build\" \"yarn run webpack\"",
    "build:full:compile": "concurrently \"yarn run build:compile\" \"yarn run webpack\"",
    "build:test": "tsc --project ./src/test/tsconfig.json",
    "clean": "rimraf dist lib *.tsbuildinfo *.build.log",
    "eslint": "eslint --format stylish src",
    "eslint:fix": "eslint --format stylish src --fix",
    "lint": "yarn run eslint",
    "lint:fix": "yarn run eslint:fix",
    "prepack": "yarn run webpack",
    "tsc": "tsc",
    "tsfmt": "tsfmt --verify",
    "tsfmt:fix": "tsfmt --replace",
    "webpack": "webpack --env=\"production\"",
    "webpack:dev": "webpack --env=\"development\""
  },
  "dependencies": {
    "@fluid-example/client-ui-lib": "^0.32.0",
    "@fluid-example/flow-util-lib": "^0.32.0",
    "@fluid-example/fluid-object-interfaces": "^0.32.0",
    "@fluid-example/search-menu": "^0.32.0",
    "@fluidframework/core-interfaces": "^0.32.0",
    "@fluidframework/data-object-base": "^0.32.0",
    "@fluidframework/datastore": "^0.32.0",
    "@fluidframework/map": "^0.32.0",
    "@fluidframework/merge-tree": "^0.32.0",
    "@fluidframework/runtime-definitions": "^0.32.0",
    "@fluidframework/sequence": "^0.32.0",
    "@fluidframework/view-interfaces": "^0.32.0",
    "debug": "^4.1.1",
    "katex": "^0.10.2"
  },
  "devDependencies": {
    "@fluidframework/build-common": "^0.20.0",
    "@fluidframework/eslint-config-fluid": "^0.22.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.17.24",
    "@typescript-eslint/eslint-plugin": "~4.2.0",
    "@typescript-eslint/parser": "~4.2.0",
    "concurrently": "^5.2.0",
    "copyfiles": "^2.1.0",
    "css-loader": "^1.0.0",
    "eslint": "~7.11.0",
    "eslint-plugin-eslint-comments": "~3.2.0",
    "eslint-plugin-import": "~2.22.0",
    "eslint-plugin-no-null": "~1.0.2",
    "eslint-plugin-prefer-arrow": "~1.2.2",
    "eslint-plugin-react": "~7.21.2",
    "eslint-plugin-unicorn": "~22.0.0",
    "file-loader": "^3.0.1",
    "rimraf": "^2.6.2",
    "style-loader": "^1.0.0",
    "ts-loader": "^6.1.2",
    "typescript": "~3.7.4",
    "typescript-formatter": "7.1.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.1.4"
  },
  "fluid": {
    "browser": {
      "umd": {
        "files": [
          "dist/main.bundle.js"
        ],
        "library": "main"
      }
    }
  }
}
