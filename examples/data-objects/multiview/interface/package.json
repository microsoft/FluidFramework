		"rimraf": "^4.4.0",
		"ts-loader": "^9.3.0",
		"typescript": "~4.5.5",
		"webpack": "^5.76.2",
		"webpack-cli": "^4.9.2",
		"webpack-dev-server": "~4.6.0",
		"webpack-merge": "^5.8.0"
	},
	"fluid": {
		"browser": {
			"umd": {
				"files": [
					"dist/main.bundle.js"
				],
