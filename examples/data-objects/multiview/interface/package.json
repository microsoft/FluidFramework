    "eslint-plugin-react": "~7.21.2",
    "eslint-plugin-unicorn": "~22.0.0",
    "puppeteer": "^1.20.0",
    "rimraf": "^2.6.2",
    "ts-loader": "^6.1.2",
    "typescript": "~3.7.4",
    "typescript-formatter": "7.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.1.4"
  },
  "fluid": {
    "browser": {
      "umd": {
