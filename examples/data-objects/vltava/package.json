    "html-loader": "^0.5.5",
    "jest": "^26.6.3",
    "jest-junit": "^10.0.0",
    "jest-puppeteer": "^4.3.0",
    "puppeteer": "^1.20.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
