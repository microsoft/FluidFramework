    "@fluidframework/runtime-definitions": "^0.39.2",
    "@fluidframework/runtime-utils": "^0.39.2",
    "@fluidframework/view-adapters": "^0.39.2",
    "@fluidframework/view-interfaces": "^0.39.2",
    "office-ui-fabric-react": "^7.28.1",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
