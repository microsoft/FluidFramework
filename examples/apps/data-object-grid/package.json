{
	"name": "@fluid-example/data-object-grid",
	"version": "2.0.0-internal.4.1.0",
	"private": true,
	"description": "Data object grid creates child data objects from a registry and lays them out in a grid.",
	"homepage": "https://fluidframework.com",
	"repository": {
		"type": "git",
