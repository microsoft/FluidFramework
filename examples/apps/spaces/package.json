{
  "name": "@fluid-example/spaces",
  "version": "1.4.0",
  "private": true,
  "description": "Spaces is a grid layout component",
  "homepage": "https://fluidframework.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/FluidFramework.git",
    "directory": "examples/apps/spaces"
  },
  "license": "MIT",
  "author": "Microsoft and contributors",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "concurrently npm:build:compile npm:lint",
    "build:compile": "concurrently npm:tsc npm:build:copy",
    "build:copy": "copyfiles -u 1 \"src/**/*.css\" dist/",
    "build:full": "concurrently npm:build npm:webpack",
    "build:full:compile": "concurrently npm:build:compile npm:webpack",
    "clean": "rimraf dist lib *.tsbuildinfo *.build.log",
    "dev": "npm run webpack:dev",
    "eslint": "eslint --format stylish src",
    "eslint:fix": "eslint --format stylish src --fix --fix-type problem,suggestion,layout",
    "lint": "npm run eslint",
    "lint:fix": "npm run eslint:fix",
    "prepack": "npm run webpack",
    "start": "webpack serve",
    "start:server": "tinylicious",
    "start:test": "webpack serve --config webpack.test.js",
    "test": "npm run test:jest",
    "test:jest": "jest",
    "test:jest:verbose": "cross-env FLUID_TEST_VERBOSE=1 jest",
    "tsc": "tsc",
    "tsfmt": "tsfmt --verify",
    "tsfmt:fix": "tsfmt --replace",
    "webpack": "webpack --env production",
    "webpack:dev": "webpack --env development"
  },
  "dependencies": {
    "@fluid-example/clicker": "^1.4.0",
    "@fluid-example/codemirror": "^1.4.0",
    "@fluid-example/collaborative-textarea": "^1.4.0",
    "@fluid-example/multiview-coordinate-model": "^1.4.0",
    "@fluid-example/multiview-slider-coordinate-view": "^1.4.0",
    "@fluid-example/prosemirror": "^1.4.0",
    "@fluid-experimental/get-container": "^1.4.0",
    "@fluidframework/aqueduct": "^1.4.0",
    "@fluidframework/common-definitions": "^0.20.1",
    "@fluidframework/core-interfaces": "^1.4.0",
    "@fluidframework/datastore-definitions": "^1.4.0",
    "@fluidframework/runtime-definitions": "^1.4.0",
    "@fluidframework/runtime-utils": "^1.4.0",
    "@fluidframework/view-adapters": "^1.4.0",
    "office-ui-fabric-react": "^7.28.1",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-collapsible": "^2.7.0",
    "react-dom": "^16.10.2",
    "react-grid-layout": "^0.18.3",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@fluidframework/build-common": "^0.24.0",
    "@fluidframework/eslint-config-fluid": "^0.28.2000",
    "@fluidframework/test-tools": "^2.0.1",
    "@rushstack/eslint-config": "^2.5.1",
    "@types/expect-puppeteer": "2.2.1",
    "@types/jest": "22.2.3",
    "@types/jest-environment-puppeteer": "2.2.0",
    "@types/node": "^14.18.0",
    "@types/prop-types": "^15",
    "@types/puppeteer": "1.3.0",
    "@types/react": "^16.9.15",
    "@types/react-dom": "^16.9.4",
    "concurrently": "^6.2.0",
    "copyfiles": "^2.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^1.0.0",
    "eslint": "~8.6.0",
    "html-loader": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^26.6.3",
    "jest-junit": "^10.0.0",
    "jest-puppeteer": "^4.3.0",
    "process": "^0.11.10",
    "puppeteer": "^1.20.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^2.0.0",
    "style-loader": "^1.0.0",
    "ts-loader": "^9.3.0",
    "typescript": "~4.5.5",
    "typescript-formatter": "7.1.0",
    "url-loader": "^2.1.0",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "~4.6.0",
    "webpack-merge": "^5.8.0"
  },
  "fluid": {
    "browser": {
      "umd": {
        "files": [
          "dist/main.bundle.js"
        ],
        "library": "main"
      }
    }
  },
  "jest-junit": {
    "outputDirectory": "nyc",
    "outputName": "jest-junit-report.xml"
  }
}
