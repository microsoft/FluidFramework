{
  "name": "@fluid-example/spaces",
  "version": "0.29.0",
  "description": "Spaces is a grid layout component",
  "homepage": "https://fluidframework.com",
  "repository": "microsoft/FluidFramework",
  "license": "MIT",
  "author": "Microsoft",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "concurrently npm:build:compile npm:lint",
    "build:compile": "concurrently npm:tsc npm:build:copy",
    "build:copy": "copyfiles -u 1 \"src/**/*.css\" dist/",
    "build:full": "concurrently npm:build npm:webpack",
    "build:full:compile": "concurrently npm:build:compile npm:webpack",
    "clean": "rimraf dist lib *.tsbuildinfo *.build.log",
    "dev": "npm run webpack:dev",
    "eslint": "eslint --format stylish src",
    "eslint:fix": "eslint --ext=ts,tsx --format stylish src --fix",
    "lint": "npm run eslint",
    "lint:fix": "npm run eslint:fix",
    "prepack": "npm run webpack",
    "start": "webpack-dev-server",
    "start:server": "tinylicious",
    "start:test": "webpack-dev-server --config webpack.test.js",
    "test": "npm run test:jest",
    "test:jest": "jest",
    "test:jest:verbose": "cross-env FLUID_TEST_VERBOSE=1 jest",
    "tsc": "tsc",
    "tsfmt": "tsfmt --verify",
    "tsfmt:fix": "tsfmt --replace",
    "webpack": "webpack --env.production",
    "webpack:dev": "webpack --env.development"
  },
  "dependencies": {
    "@fluid-example/clicker": "^0.29.0",
    "@fluid-example/codemirror": "^0.29.0",
    "@fluid-example/collaborative-textarea": "^0.29.0",
    "@fluid-example/multiview-coordinate-model": "^0.29.0",
    "@fluid-example/multiview-slider-coordinate-view": "^0.29.0",
    "@fluid-example/prosemirror": "^0.29.0",
    "@fluidframework/aqueduct": "^0.29.0",
    "@fluidframework/core-interfaces": "^0.29.0",
    "@fluidframework/datastore-definitions": "^0.29.0",
    "@fluidframework/get-tinylicious-container": "^0.29.0",
    "@fluidframework/runtime-definitions": "^0.29.0",
    "@fluidframework/runtime-utils": "^0.29.0",
    "@fluidframework/view-adapters": "^0.29.0",
    "@fluidframework/view-interfaces": "^0.29.0",
    "office-ui-fabric-react": "^7.28.1",
    "react": "^16.10.2",
    "react-collapsible": "^2.7.0",
    "react-dom": "^16.10.2",
    "react-grid-layout": "^0.18.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@fluidframework/build-common": "^0.19.2",
    "@fluidframework/eslint-config-fluid": "^0.21.0-0",
    "@fluidframework/get-session-storage-container": "^0.29.0",
    "@fluidframework/test-tools": "^0.2.3074",
    "@types/node": "^10.17.24",
    "@types/react": "^16.9.15",
    "@types/react-dom": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "~4.2.0",
    "@typescript-eslint/parser": "~4.2.0",
    "concurrently": "^5.2.0",
    "copyfiles": "^2.1.0",
    "cross-env": "^7.0.2",
    "css-loader": "^1.0.0",
    "eslint": "~7.9.0",
    "eslint-plugin-eslint-comments": "~3.2.0",
    "eslint-plugin-import": "~2.22.0",
    "eslint-plugin-no-null": "~1.0.2",
    "eslint-plugin-prefer-arrow": "~1.2.2",
    "eslint-plugin-react": "~7.21.2",
    "eslint-plugin-unicorn": "~22.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.4.2",
    "jest-junit": "^10.0.0",
    "jest-puppeteer": "^4.3.0",
    "puppeteer": "^1.20.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.0.0",
    "ts-loader": "^6.1.2",
    "typescript": "~3.7.4",
    "typescript-formatter": "7.1.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.1.4"
  },
  "fluid": {
    "browser": {
      "umd": {
        "files": [
          "dist/main.bundle.js"
        ],
        "library": "main"
      }
    }
  },
  "jest-junit": {
    "outputDirectory": "nyc",
    "outputName": "jest-junit-report.xml"
  }
}
