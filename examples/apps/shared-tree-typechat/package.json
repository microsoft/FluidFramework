{
	"name": "planner",
	"version": "0.0.1",
	"private": "true",
	"description": "",
	"homepage": "https://fluidframework.com",
	"repository": {
		"type": "git",
		"url": "https://github.com/microsoft/FluidFramework.git",
		"directory": "examples/apps/shared-tree-typechat"
	},
	"license": "MIT",
	"author": "Microsoft and contributors",
	"type": "module",
	"scripts": {
		"build": "fluid-build . --task build",
		"build:compile": "fluid-build . --task compile",
		"build:esnext": "tsc --project ./tsconfig.json",
		"check:biome": "biome check .",
		"check:format": "npm run check:biome",
		"check:prettier": "prettier --check . --cache --ignore-path ../../../.prettierignore",
		"ci:test": "start-server-and-test start:server 7070 ci:test:jest",
		"ci:test:jest": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --ci --reporters=default --reporters=jest-junit",
		"clean": "rimraf --glob dist lib \"**/*.tsbuildinfo\" \"**/*.build.log\" nyc",
		"compile": "tsc -b",
		"dev": "cross-env webpack-dev-server",
		"dev:azure": "cross-env FLUID_CLIENT=azure webpack-dev-server",
		"dev:local": "cross-env FLUID_CLIENT=local webpack-dev-server",
		"dev:spe": "cross-env FLUID_CLIENT=spe webpack-dev-server",
		"eslint": "eslint --format stylish src",
		"eslint:fix": "eslint --format stylish src --fix --fix-type problem,suggestion,layout",
		"format": "npm run format:biome",
		"format:biome": "biome check . --write",
		"format:prettier": "prettier --write . --cache --ignore-path ../../../.prettierignore",
		"lint": "fluid-build . --task lint",
		"lint:fix": "fluid-build . --task eslint:fix --task format",
		"start": "npm run dev",
		"start:server": "npx @fluidframework/azure-local-service",
		"test": "npm run test:jest",
		"test:jest": "jest",
		"test:jest:verbose": "cross-env FLUID_TEST_VERBOSE=1 jest --passWithNoTests",
		"webpack": "webpack --env production",
		"webpack:dev": "webpack --env development"
	},
	"dependencies": {
		"@azure/identity": "^4.0.0",
		"@azure/msal-browser": "^3.13.0",
		"@fluentui/react-components": "^9.48.0",
		"@fluentui/react-icons": "^2.0.221",
		"@fluidframework/azure-client": "workspace:~",
		"@fluidframework/odsp-client": "workspace:~",
		"@fluidframework/telemetry-utils": "workspace:~",
		"@fluidframework/test-runtime-utils": "workspace:~",
		"@headlessui/react": "^1.7.18",
		"@microsoft/microsoft-graph-client": "^3.0.7",
		"@types/react": "^18.2.0",
		"@types/react-dom": "^18.2.0",
		"axios": "^1.6.2",
		"dotenv": "^16.0.2",
		"fluid-framework": "workspace:~",
		"hashids": "^2.2.10",
		"jsrsasign": "^11.1.0",
		"react": "^18.2.0",
		"react-dnd": "^16.0.1",
		"react-dnd-html5-backend": "^16.0.1",
		"react-dom": "^18.2.0",
		"react-transition-state": "^2.1.0",
		"typechat": "^0.1.0",
		"unique-names-generator": "^4.7.1",
		"uuid": "^9.0.1"
	},
	"devDependencies": {
		"@fluidframework/azure-local-service": "workspace:~",
		"@fluidframework/build-common": "^2.0.3",
		"@fluidframework/devtools": "workspace:~",
		"@microsoft/microsoft-graph-types": "^2.40.0",
		"@types/debug": "^4.1.7",
		"@types/jsrsasign": "^10.5.13",
		"@types/uuid": "^9.0.8",
		"@typescript-eslint/eslint-plugin": "^5.38.0",
		"@typescript-eslint/parser": "^5.38.0",
		"copy-webpack-plugin": "^11.0.0",
		"cross-env": "^7.0.3",
		"css-loader": "^5",
		"dotenv-webpack": "^7.1.1",
		"eslint": "^8.23.1",
		"eslint-plugin-react": "^7.31.8",
		"html-webpack-plugin": "^4.5.2",
		"jest": "^29.2.2",
		"jest-junit": "^16.0.0",
		"jest-puppeteer": "^9.0.0",
		"mini-css-extract-plugin": "^1",
		"prettier": "^2.7.1",
		"puppeteer": "^21.0.0",
		"start-server-and-test": "^2.0.0",
		"style-loader": "^2",
		"tailwindcss": "^3.3.2",
		"ts-loader": "^8.0.0",
		"typescript": "~5.4",
		"webpack": "^5.88.2",
		"webpack-cli": "^5.1.4",
		"webpack-dev-server": "^4.15.1"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"jest-junit": {
		"outputDirectory": "nyc",
		"outputName": "jest-junit-report.xml"
	}
}
