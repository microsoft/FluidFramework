{
  "name": "@fluid-example/bundle-size-tests",
  "version": "0.32.0",
  "description": "A package for understanding the bundle size of Fluid Framework",
  "homepage": "https://fluidframework.com",
  "repository": "https://github.com/microsoft/FluidFramework",
  "license": "MIT",
  "author": "Microsoft",
  "scripts": {
    "build": "concurrently \"yarn run build:compile\" \"yarn run lint\"",
    "build:compile": "tsc",
    "build:full": "concurrently \"yarn run build\" \"yarn run webpack\"",
    "build:full:compile": "concurrently \"yarn run build:compile\" \"yarn run webpack\"",
    "clean": "rimraf dist lib *.tsbuildinfo *.build.log",
    "eslint": "eslint --format stylish src",
    "eslint:fix": "eslint --format stylish src --fix",
    "lint": "yarn run eslint",
    "lint:fix": "yarn run eslint:fix",
    "prepack": "yarn run webpack",
    "tsfmt": "tsfmt --verify",
    "tsfmt:fix": "tsfmt --replace",
    "webpack": "webpack",
    "webpack:profile": "yarn run webpack"
  },
  "dependencies": {
    "@fluidframework/container-runtime": "^0.32.0",
    "@fluidframework/map": "^0.32.0",
    "@fluidframework/matrix": "^0.32.0",
    "@fluidframework/odsp-driver": "^0.32.0",
    "@fluidframework/sequence": "^0.32.0"
  },
  "devDependencies": {
    "@fluidframework/build-common": "^0.20.0",
    "@fluidframework/bundle-size-tools": "^0.0.8505",
    "@fluidframework/eslint-config-fluid": "^0.22.0",
    "@mixer/webpack-bundle-compare": "^0.1.0",
    "@types/node": "^10.17.24",
    "@types/puppeteer": "1.3.0",
    "@typescript-eslint/eslint-plugin": "~4.2.0",
    "@typescript-eslint/parser": "~4.2.0",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "eslint": "~7.11.0",
    "eslint-plugin-eslint-comments": "~3.2.0",
    "eslint-plugin-import": "~2.22.0",
    "eslint-plugin-no-null": "~1.0.2",
    "eslint-plugin-prefer-arrow": "~1.2.2",
    "eslint-plugin-react": "~7.21.2",
    "eslint-plugin-unicorn": "~22.0.0",
    "rimraf": "^2.6.2",
    "ts-loader": "^6.1.2",
    "typescript": "~3.7.4",
    "typescript-formatter": "7.1.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11"
  }
}
