<?xml version="1.0" encoding="utf-8"?> 
<AutoVisualizer xmlns="http://schemas.microsoft.com/vstudio/debugger/natvis/2010">
  <Type Name="LengthMap::Entry">
    <DisplayString Condition="seq == 0">{{Seq = Universal}}</DisplayString>
    <DisplayString Condition="seq == Seq::Invalid()">{{Seq = Invalid}}</DisplayString>
    <DisplayString>{{Seq = {seq}}}</DisplayString>
    <Expand>
      <ExpandedItem>lengths</ExpandedItem>
    </Expand>
  </Type>
</AutoVisualizer>