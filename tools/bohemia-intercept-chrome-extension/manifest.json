{
  "name": "Bohemia Intercept Chrome Extension",
  "version": "1.0",
  "description": "Inject a Fluid component into Bohemia",
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "https://*.fluidpreview.office.net/*",
    "https://*.microsoft.sharepoint-df.com/*",
    "https://*.microsofteur-my.sharepoint.com/*",
    "http://localhost:8080/*",
    "http://localhost:3000/*"
  ],
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  },
  "devtools_page": "devtools.html",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "web_accessible_resources": [
    "localhost:3000",
    "localhost:8080"
  ]
}
