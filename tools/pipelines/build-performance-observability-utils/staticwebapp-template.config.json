{
  "auth": {
    "identityProviders": {
      "azureActiveDirectory": {
        "login": {
          "loginParameters": ["scope=openid"]
        }
      }
    }
  },
  "routes": [
    {
      "route": "/*",
      "allowedRoles": ["anonymous", "authenticated"]
    }
  ],
  "globalHeaders": {
    "Cache-Control": "no-cache"
  }
}
