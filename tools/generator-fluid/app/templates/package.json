{
  "name": "@yo-fluid/dice-roller",
  "version": "0.18.0",
  "description": "Minimal Fluid example to implement a collaborative dice roller.",
  "repository": "microsoft/FluidFramework",
  "author": "Microsoft",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "webpack --env.production && npm run tsc",
    "dev": "webpack --env.development",
    "start": "webpack-dev-server --config webpack.config.js --package package.json --env.mode local",
    "start:localhost": "webpack-dev-server --config webpack.config.js --package package.json --env.mode localhost",
    "start:r11s": "webpack-dev-server --config webpack.config.js --package package.json --env.mode r11s",
    "start:spo": "webpack-dev-server --config webpack.config.js --package package.json --env.mode spo",
    "start:spo-df": "webpack-dev-server --config webpack.config.js --package package.json --env.mode spo-df",
    "tsc": "tsc",
    "webpack": "npm run build",
    "webpack:dev": "npm run dev"
  },
  "dependencies": {
    "@microsoft/fluid-aqueduct": "^0.18.0-0",
    "@microsoft/fluid-map": "^0.18.0-0",
    "@microsoft/fluid-view-interfaces": "^0.18.0-0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3"
  },
  "devDependencies": {
    "@microsoft/fluid-webpack-component-loader": "^0.18.0-0",
    "@types/node": "^10.14.6",
    "@types/react-dom": "^16.0.11",
    "ts-loader": "^4.5.0",
    "typescript": "~3.7.4",
    "webpack": "^4.43.0",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "4.1.4"
  },
  "fluid": {
    "browser": {
      "umd": {
        "files": [
          "dist/main.bundle.js"
        ],
        "library": "main"
      }
    }
  }
}
