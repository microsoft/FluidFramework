{
  "name": "@fluid-internal/build-cli",
  "version": "0.1.0",
  "private": true,
  "description": "",
  "keywords": [
    "oclif"
  ],
  "homepage": "https://fluidframework.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/FluidFramework.git",
    "directory": "tools/build-cli"
  },
  "license": "MIT",
  "author": "Microsoft and contributors",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "flub": "./bin/run"
  },
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "build:full": "npm run format && npm run prepack",
    "format": "npm run prettier:fix",
    "lint": "eslint ./src --ext .ts --config .eslintrc.cjs",
    "prepack": "npm run build && oclif manifest && oclif readme",
    "postpack": "shx rm -f oclif.manifest.json",
    "prettier": "prettier --check \"**/*.{cjs,js,json,jsx,ts,tsx,yml,yaml}\"",
    "prettier:fix": "prettier --write \"**/*.{cjs,js,json,jsx,ts,tsx,yml,yaml}\"",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "npm run lint",
    "tsc": "tsc -b",
    "version": "oclif readme && git add README.md"
  },
  "dependencies": {
    "@fluidframework/build-tools": "^0.2.76221",
    "@oclif/core": "^1.9.5",
    "@oclif/plugin-help": "^5",
    "@oclif/plugin-not-found": "^2.3.1",
    "@oclif/plugin-plugins": "^2.0.1",
    "@oclif/test": "^2",
    "semver": "^7.3.7"
  },
  "devDependencies": {
    "@fluidframework/eslint-config-fluid": "^0.28.2000",
    "@types/chai": "^4",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.9.4",
    "@types/semver": "^7.3.10",
    "@typescript-eslint/eslint-plugin": "~5.9.0",
    "@typescript-eslint/parser": "~5.9.0",
    "chai": "^4",
    "eslint": "~8.6.0",
    "eslint-config-oclif": "^4",
    "eslint-config-oclif-typescript": "^1.0.2",
    "eslint-config-prettier": "~8.5.0",
    "eslint-plugin-eslint-comments": "~3.2.0",
    "eslint-plugin-import": "~2.25.4",
    "eslint-plugin-unicorn": "~40.0.0",
    "eslint-plugin-unused-imports": "~2.0.0",
    "globby": "^11",
    "mocha": "^9",
    "oclif": "^3",
    "prettier": "~2.6.2",
    "shx": "^0.3.3",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "oclif": {
    "bin": "flub",
    "dirname": "tools/build-cli",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help"
    ],
    "topicSeparator": " ",
    "topics": {}
  }
}
