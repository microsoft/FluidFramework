{
    "Definitions": {
        "dotGroup": false,
        "layers": {
            "Base-Definitions": {
                "dotSameRank": true,
                "packages": [
                    "@fluidframework/common-definitions",
                    "@fluidframework/core-interfaces",
                    "@fluidframework/gitresources"
                ]
            },
            "Protocol-Definitions": {
                "packages": [
                    "@fluidframework/protocol-definitions"
                ],
                "deps": [
                    "@fluidframework/common-definitions"
                ]
            },
            "Driver-Definitions": {
                "packages": [
                    "@fluidframework/driver-definitions",
                    "@fluidframework/odsp-driver-definitions"
                ],
                "deps": [
                    "Protocol-Definitions",
                    "@fluidframework/core-interfaces"
                ]
            },
            "Container-Definitions": {
                "packages": [
                    "@fluidframework/container-definitions"
                ],
                "deps": [
                    "Driver-Definitions",
                    "@fluidframework/core-interfaces"
                ]
            }
        }
    },
    "Utils": {
        "layers": {
            "Base-Utils": {
                "packages": [
                    "@fluidframework/common-utils"
                ],
                "deps": [
                    "Base-Definitions"
                ]
            },
            "Protocol-Utils": {
                "packages": [
                    "@fluidframework/protocol-base"
                ],
                "deps": [
                    "Base-Utils",
                    "Protocol-Definitions"
                ]
            },
            "Driver-Utils": {
                "packages": [
                    "@fluidframework/driver-utils"
                ],
                "deps": [
                    "Protocol-Utils",
                    "Driver-Definitions",
                    "Telemetry-Utils"
                ]
            },
            "Framework-Utils": {
                "packages": [
                    "@fluidframework/view-adapters",
                    "@fluidframework/view-interfaces"
                ],
                "deps": [
                    "Base-Definitions"
                ]
            },
            "Other-Utils": {
                "dot": false,
                "packages": [
                    "@fluidframework/odsp-doclib-utils"
                ],
                "deps": [
                    "Driver-Definitions",
                    "Driver-Utils"
                ]
            },
            "Tool-Utils": {
                "dot": false,
                "packages": [
                    "@fluidframework/tool-utils"
                ],
                "deps": [
                    "Base-Utils",
                    "Other-Utils"
                ]
            },
            "Telemetry-Utils": {
                "packages": [
                    "@fluidframework/telemetry-utils"
                ],
                "deps": [
                    "Base-Utils",
                    "Base-Definitions"
                ]
            },
            "Container-Utils": {
                "packages": [
                    "@fluidframework/container-utils"
                ],
                "deps": [
                    "Telemetry-Utils",
                    "Container-Definitions"
                ]
            }
        }
    },
    "Client": {
        "dotGroup": false,
        "layers": {
            "Driver": {
                "dirs": [
                    "packages/drivers/"
                ],
                "deps": [
                    "Driver-Utils",
                    "@fluidframework/core-interfaces",
                    "Other-Utils"
                ]
            },
            "Loader": {
                "dirs": [
                    "packages/loader/"
                ],
                "deps": [
                    "Container-Definitions",
                    "Driver-Utils",
                    "Container-Utils"
                ]
            },
            "Hosts": {
                "packages": [
                    "@fluidframework/base-host"
                ],
                "deps": [
                    "Loader",
                    "Framework-Utils"
                ]
            },
            "HostUtils": {
                "dev": true,
                "packages": [
                    "@fluid-experimental/frs-client",
                    "@fluid-experimental/get-container",
                    "@fluid-experimental/tinylicious-client"                  
                ],
                "deps": [
                    "Framework",
                    "Loader",
                    "Routerlicious-Driver",
                    "Server-Libs",
                    "Server-Shared-Utils",
                    "Test"                    
                ]
            },
            "Runtime": {
                "packages": [
                    "@fluidframework/agent-scheduler"
                ],
                "dirs": [
                    "experimental/dds/",
                    "packages/dds/",
                    "packages/runtime/"
                ],
                "deps": [
                    "Container-Definitions",
                    "Driver-Utils",
                    "Container-Utils"
                ]
            },
            "Framework": {
                "packages": [
                    "@fluid-internal/client-api",
                    "@fluid-experimental/fluid-framework",
                    "@fluid-experimental/data-objects",
                    "@fluid-experimental/fluid-static"
                ],
                "dirs": [
                    "experimental/framework/",
                    "experimental/PropertyDDS/packages/",
                    "packages/framework/"
                ],
                "deps": [
                    "Hosts",
                    "Runtime"
                ]
            },
            "Examples": {
                "dot": false,
                "dev": true,
                "dirs": [
                    "examples/",
                    "experimental/examples/",
                    "experimental/PropertyDDS/examples/",
                    "experimental/PropertyDDS/services/",
                    "packages/agents/"
                ],
                "deps": [
                    "Framework",
                    "Driver",
                    "Routerlicious-Driver",
                    "Test"
                ]
            },
            "Build": {
                "dev": true,
                "packages": [
                    "@fluidframework/build-common",
                    "@fluidframework/eslint-config-fluid",
                    "@fluid-tools/benchmark"
                ]
            },
            "Tools": {
                "dot": false,
                "dirs": [
                    "packages/tools/",
                    "tools/generator-fluid"
                ],
                "deps": [
                    "Framework",
                    "Routerlicious-Driver",
                    "Other-Utils",
                    "Tool-Utils"
                ]
            },
            "Test": {
                "dev": true,
                "packages": [
                    "@fluid-internal/test-dds-utils",
                    "@fluidframework/local-driver",
                    "@fluidframework/test-runtime-utils",
                    "@fluidframework/webpack-fluid-loader"
                ],
                "dirs": [
                    "packages/test/"
                ]
            }
        }
    },
    "Server": {
        "layers": {
            "Server-Shared-Utils": {
                "packages": [
                    "@fluidframework/server-services-client"
                ],
                "deps": [
                    "Protocol-Utils"
                ]
            },
            "Server-Libs": {
                "dirs": [
                    "server/routerlicious/packages/"
                ],
                "deps": [
                    "Protocol-Utils",
                    "Server-Shared-Utils"
                ]
            },
            "Server-Tools": {
                "packages": [
                    "tinylicious"
                ],
                "deps": [
                    "Server-Libs"
                ]
            }
        }
    },
    "Routerlicious": {
        "dot": false,
        "layers": {
            "Routerlicious-Driver": {
                "packages": [
                    "@fluidframework/routerlicious-driver",
                    "@fluidframework/routerlicious-urlresolver",
                    "@fluidframework/tinylicious-driver"
                ],
                "deps": [
                    "Driver",
                    "Server-Shared-Utils"
                ]
            },
            "Routerlicious-Server": {
                "packages": [
                    "@fluidframework/server-routerlicious"
                ],
                "deps": [
                    "Server-Libs"
                ]
            }
        }
    }
}
