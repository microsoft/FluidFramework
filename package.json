{
	"name": "@intentional/shared-tree",
	"version": "0.0.0-placeholder",
	"description": "Microsoft Whiteboard collaboration libraries",
	"bugs": {
		"url": "https://dev.azure.com/intentional/intent/_workitems"
	},
	"repository": {
		"type": "git",
		"url": "https://dev.azure.com/intentional/intent/_git/typescript-pipe"
	},
	"license": "MIT",
	"author": "Microsoft",
	"sideEffects": false,
	"main": "dist/index.js",
	"module": "lib/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "npm run build:compile && npm run build:docs",
		"build:code": "npm run build:compile",
		"build:compile": "concurrently npm:tsc npm:build:esnext",
		"build:docs": "api-extractor run --local && copyfiles -u 2 ./_api-extractor-temp/doc-models/* ../../api-metadata/",
		"build:esnext": "tsc --project ./tsconfig.esnext.json",
		"build:full": "npm run build",
		"build:full:compile": "npm run build:compile",
		"clean": "rimraf dist lib *.tsbuildinfo *.build.log _api-extractor-temp",
		"eslint": "eslint src",
		"eslint:fix": "eslint src --fix",
		"lint": "npm run eslint",
		"lint:fix": "npm run eslint:fix",
		"perf": "env-cmd -e production mocha \"dist/**/*.tests.js\" --config ./.mocharc.js --expose-gc --perfMode --fgrep @Benchmark --reporter @fluid-tools/benchmark/dist/MochaReporter.js",
		"test": "npm run test:mocha",
		"test:mocha": "nyc --report-dir='./coverage' mocha \"dist/**/*.tests.js\" --config ./.mocharc.js --unhandled-rejections=strict",
		"tsc": "tsc --pretty"
	},
	"dependencies": {
		"@fluidframework/common-definitions": "^0.20.1",
		"@fluidframework/common-utils": "^0.32.1",
		"@fluidframework/container-definitions": "^0.39.8",
		"@fluidframework/core-interfaces": "^0.39.8",
		"@fluidframework/datastore-definitions": "^0.47.0",
		"@fluidframework/protocol-definitions": "^0.1024.0",
		"@fluidframework/shared-object-base": "^0.47.0",
		"@fluidframework/telemetry-utils": "^0.47.0",
		"@fluidframework/undo-redo": "^0.47.0",
		"buffer": "^6.0.3",
		"denque": "^1.5.1",
		"lru-cache": "^6.0.0",
		"sorted-btree": "^1.6.0",
		"uuid": "^8.3.2"
	},
	"devDependencies": {
		"@fluid-tools/benchmark": "^0.40.27919",
		"@fluidframework/build-common": "^0.22.0",
		"@fluidframework/container-definitions": "^0.39.8",
		"@fluidframework/container-loader": "^0.47.0",
		"@fluidframework/eslint-config-fluid": "^0.23.0",
		"@fluidframework/runtime-definitions": "^0.47.0",
		"@fluidframework/runtime-utils": "^0.47.0",
		"@fluidframework/test-driver-definitions": "^0.47.0",
		"@fluidframework/test-drivers": "^0.47.0",
		"@fluidframework/test-runtime-utils": "^0.47.0",
		"@fluidframework/test-utils": "^0.47.0",
		"@microsoft/api-extractor": "7.18.8",
		"@types/lru-cache": "^5.1.1",
		"@types/socket.io-client": "^1.4.36",
		"@typescript-eslint/parser": "^4.33.0",
		"chai": "^4.3.4",
		"concurrently": "^6.3.0",
		"copyfiles": "^2.4.1",
		"env-cmd": "^10.1.0",
		"eslint": "^7.32.0",
		"mocha": "^8.4.0",
		"nyc": "^15.1.0",
		"prando": "^6.0.1",
		"prettier": "^2.4.1",
		"rimraf": "^3.0.2"
	},
	"publishConfig": {
		"registry": "https://pkgs.dev.azure.com/intentional/_packaging/intentional-npm/npm/registry/"
	}
}
