{
	// This file is not used in compilation. It is here just for a nice editor experience.
	"extends": "@docusaurus/tsconfig",
	"compilerOptions": {
		"strict": true,
		"strictNullChecks": true,
		"forceConsistentCasingInFileNames": true,
		"baseUrl": ".",
		"types": ["react", "react-dom", "node"],
		"lib": ["dom", "es2020"],
	},
	"include": ["src"],
	"exclude": ["test"],
	"imports": {
		"@site/src/*": "./src/*",
		"@site/static/*": "./static/*",
	},
}
