{
  "name": "@microsoft/api-documenter",
  "version": "7.8.22",
  "description": "Read JSON files from api-extractor, generate documentation pages",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/rushstack/tree/master/apps/api-documenter"
  },
  "homepage": "https://api-extractor.com/",
  "license": "MIT",
  "scripts": {
    "build": "gulp test --clean",
    "api:tomarkdown": "bin/api-documenter generate --input-folder ../../../_api-extractor-temp/doc-models/ --output-folder ../../content/apis/"
  },
  "bin": {
    "api-documenter": "./bin/api-documenter"
  },
  "main": "lib/index.js",
  "typings": "dist/rollup.d.ts",
  "dependencies": {
    "@microsoft/api-documenter": "7.8.22",
    "@microsoft/api-extractor-model": "~7.8.13",
    "@rushstack/node-core-library": "~3.26.1",
    "@microsoft/tsdoc": "0.12.19",
    "colors": "~1.2.1",
    "js-yaml": "~3.13.1",
    "resolve": "~1.17.0",
    "doc-plugin-fluid": "file:../doc-plugin-fluid"
  },
  "devDependencies": {
    "@microsoft/rush-stack-compiler-3.5": "~0.8.2",
    "@microsoft/node-library-build": "~6.4.33",
    "@rushstack/eslint-config": "1.0.3",
    "@types/jest": "25.2.1",
    "@types/js-yaml": "3.12.1",
    "@types/node": "10.17.13",
    "@types/resolve": "1.17.1",
    "gulp": "~4.0.2",
    "jest": "~25.4.0"
  }
}
