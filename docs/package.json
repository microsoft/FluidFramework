{
	"name": "fluid-framework-docs-site",
	"version": "0.0.0",
	"private": true,
	"homepage": "https://fluidframework.com",
	"repository": {
		"type": "git",
		"url": "https://github.com/microsoft/FluidFramework.git",
		"directory": "docs"
	},
	"license": "MIT",
	"author": "Microsoft and contributors",
	"scripts": {
		"api-markdown-documenter": "node ./infra/generate-api-documentation.mjs",
		"build": "npm run build:api-documentation && npm run build:docusaurus && npm run build:copy-azure-assets",
		"build:api-documentation": "npm run download-doc-models && npm run api-markdown-documenter",
		"build:api-documentation:dev": "cross-env NODE_ENV=development npm run build:api-documentation",
		"build:copy-azure-assets": "copyfiles -u 2 \"./infra/azure/**/*\" \"./build/api\"",
		"build:dev": "cross-env NODE_ENV=development npm run build",
		"build:docusaurus": "npm run gen-docusaurus && docusaurus build",
		"check-links": "echo TODO: Implement linkcheck script",
		"clean": "concurrently \"npm:clean:*\"",
		"clean:api-documentation": "rimraf --glob \"docs/api/**/*.md\" \"versioned_docs/*/api/**/*.md\"",
		"clean:doc-models": "rimraf --glob .doc-models",
		"clean:docusaurus": "docusaurus clear",
		"deploy": "npm run gen-docusaurus && docusaurus deploy",
		"download-doc-models": "node ./infra/download-doc-models.mjs",
		"eslint": "eslint --format stylish src",
		"eslint:fix": "eslint --format stylish src --fix",
		"format": "npm run prettier:fix",
		"gen-docusaurus": "node ./infra/pre-docusaurus.mjs",
		"lint": "concurrently npm:eslint npm:prettier",
		"lint:fix": "npm run eslint:fix && npm run prettier:fix",
		"prettier": "prettier --check ./src --cache --ignore-path ../.prettierignore",
		"prettier:fix": "prettier --write ./src --cache --ignore-path ../.prettierignore",
		"rebuild": "npm run clean && npm run build",
		"serve": "docusaurus serve",
		"start": "npm run gen-docusaurus && docusaurus start",
		"swizzle": "docusaurus swizzle",
		"tsc": "tsc",
		"write-heading-ids": "docusaurus write-heading-ids",
		"write-translations": "docusaurus write-translations"
	},
	"browserslist": {
		"production": [
			">0.5%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 3 chrome version",
			"last 3 firefox version",
			"last 5 safari version"
		]
	},
	"dependencies": {
		"@docusaurus/core": "^3.5.2",
		"@docusaurus/module-type-aliases": "^3.5.2",
		"@docusaurus/plugin-content-docs": "^3.5.2",
		"@docusaurus/preset-classic": "^3.5.2",
		"@docusaurus/theme-common": "^3.5.2",
		"@docusaurus/theme-mermaid": "^3.5.2",
		"@docusaurus/tsconfig": "^3.5.2",
		"@docusaurus/types": "^3.5.2",
		"@fluentui/react-components": "^9.47.5",
		"@fluentui/react-icons": "^2.0.233",
		"@mdx-js/react": "^3.0.0",
		"@rushstack/node-core-library": "^5.7.0",
		"clsx": "^2.0.0",
		"docusaurus-lunr-search": "^3.4.0",
		"docusaurus-plugin-sass": "^0.2.5",
		"lunr": "^2.3.9",
		"prism-react-renderer": "^2.3.0",
		"react": "^18.0.0",
		"react-dom": "^18.0.0",
		"sass": "^1.77.8"
	},
	"devDependencies": {
		"@fluid-tools/api-markdown-documenter": "^0.17.1",
		"@fluidframework/build-common": "^2.0.3",
		"@fluidframework/eslint-config-fluid": "^5.4.0",
		"chalk": "^5.3.0",
		"concurrently": "^8.2.2",
		"copyfiles": "^2.4.1",
		"cross-env": "^7.0.3",
		"dill-cli": "^0.1.3",
		"eslint": "~8.55.0",
		"eslint-config-prettier": "~9.0.0",
		"fs-extra": "^11.2.0",
		"prettier": "^3.2.5",
		"rimraf": "^6.0.1",
		"typescript": "~5.5.2"
	},
	"engines": {
		"node": ">=18.0"
	}
}
