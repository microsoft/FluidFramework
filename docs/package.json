{
	"name": "fluid-framework-docs-site",
	"version": "0.0.0",
	"private": true,
	"homepage": "https://fluidframework.com",
	"repository": {
		"type": "git",
		"url": "https://github.com/microsoft/FluidFramework.git",
		"directory": "docs"
	},
	"license": "MIT",
	"author": "Microsoft and contributors",
	"scripts": {
		"build": "concurrently npm:build:site npm:build:test",
		"build:api-documentation": "npm run download-doc-models && npm run generate-api-documentation",
		"prebuild:docusaurus": "npm run generate-versions",
		"build:docusaurus": "docusaurus build",
		"build:generate-content": "concurrently npm:build:markdown-magic npm:build:api-documentation",
		"build:markdown-magic": "markdown-magic",
		"build:site": "npm run build:generate-content && npm run build:docusaurus",
		"build:test": "tsc --project test/tsconfig.json --pretty --noEmit",
		"check-links": "echo TODO: Implement linkcheck script",
		"clean": "concurrently \"npm:clean:*\"",
		"clean:api-documentation": "rimraf --glob \"docs/api/**/*.md\" \"versioned_docs/*/api/**/*.md\"",
		"clean:doc-models": "rimraf --glob .doc-models",
		"clean:docusaurus": "docusaurus clear",
		"clean:test": "rimraf --glob test-results",
		"clean:versions-json": "rimraf --glob \"./versions.json\"",
		"download-doc-models": "node ./infra/download-doc-models.mjs",
		"eslint": "eslint src test --format stylish",
		"eslint:fix": "eslint src test --format stylish --fix",
		"format": "npm run prettier:fix",
		"generate-api-documentation": "dotenv -- node ./infra/generate-api-documentation.mjs",
		"generate-versions": "dotenv -- node ./infra/generate-versions.mjs",
		"lint": "concurrently npm:eslint npm:prettier",
		"lint:fix": "npm run eslint:fix && npm run prettier:fix",
		"prettier": "prettier --check . --cache --ignore-path ../.prettierignore",
		"prettier:fix": "prettier --write . --cache --ignore-path ../.prettierignore",
		"rebuild": "npm run clean && npm run build",
		"serve": "docusaurus serve",
		"serve-with-azure-emulation": "swa start build --api-location api",
		"prestart": "npm run generate-versions",
		"start": "docusaurus start",
		"pretest": "playwright install --with-deps",
		"test": "playwright test"
	},
	"browserslist": {
		"production": [
			">0.5%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 3 chrome version",
			"last 3 firefox version",
			"last 5 safari version"
		]
	},
	"dependencies": {
		"@docusaurus/core": "^3.6.0",
		"@docusaurus/module-type-aliases": "^3.6.0",
		"@docusaurus/plugin-content-docs": "^3.6.0",
		"@docusaurus/preset-classic": "^3.6.0",
		"@docusaurus/theme-common": "^3.6.0",
		"@docusaurus/theme-mermaid": "^3.6.0",
		"@docusaurus/tsconfig": "^3.6.0",
		"@docusaurus/types": "^3.6.0",
		"@rushstack/node-core-library": "^5.9.0",
		"clsx": "^2.0.0",
		"docusaurus-lunr-search": "^3.5.0",
		"docusaurus-plugin-sass": "^0.2.5",
		"lunr": "^2.3.9",
		"prism-react-renderer": "^2.4.0",
		"react": "^18.0.0",
		"react-dom": "^18.0.0"
	},
	"devDependencies": {
		"@azure/static-web-apps-cli": "^2.0.1",
		"@fluid-tools/api-markdown-documenter": "^0.17.1",
		"@fluid-tools/markdown-magic": "file:../tools/markdown-magic",
		"@fluidframework/build-common": "^2.0.3",
		"@fluidframework/eslint-config-fluid": "^5.4.0",
		"@playwright/test": "^1.48.0",
		"@types/node": "^22.9.0",
		"chalk": "^5.3.0",
		"concurrently": "^9.1.0",
		"cross-env": "^7.0.3",
		"dill-cli": "^0.1.3",
		"dotenv-cli": "^7.4.2",
		"eslint": "~8.55.0",
		"eslint-config-prettier": "~9.1.0",
		"fs-extra": "^11.2.0",
		"playwright": "^1.48.0",
		"prettier": "^3.2.5",
		"rimraf": "^6.0.1",
		"typescript": "~5.5.2"
	},
	"packageManager": "pnpm@8.15.8+sha512.d1a029e1a447ad90bc96cd58b0fad486d2993d531856396f7babf2d83eb1823bb83c5a3d0fc18f675b2d10321d49eb161fece36fe8134aa5823ecd215feed392",
	"engines": {
		"node": ">=18.0"
	}
}
