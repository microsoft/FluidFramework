{
  "name": "adminportal",
  "version": "0.0.1",
  "description": "Admin portal for prague",
  "main": "dist/www.js",
  "scripts": {
    "build": "concurrently \"npm run tslint\" \"npm run tsc && npm run webpack\" \"npm run less\"",
    "webpack": "webpack --config webpack.config.js",
    "tsc": "tsc",
    "tslint": "tslint 'src/**/*.ts*'",
    "less": "lessc src/stylesheets/style.less public/stylesheets/dist/style.css",
    "start": "docker-compose up",
    "start:debug": "docker-compose -f docker-compose.yml -f docker-compose.debug.yml up",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docker:build": "docker build --build-arg NPM_TOKEN=$(echo $NPM_TOKEN) -t admin .",
    "docker:start": "docker run --rm -t --network routerlicious_default -p 7000:3000 admin"
  },
  "dependencies": {
    "@prague/container-definitions": "^0.3.6760",
    "@prague/container-loader": "^0.3.6760",
    "@prague/map": "^0.3.6760",
    "@prague/routerlicious-host": "^0.3.6760",
    "@prague/routerlicious-socket-storage": "^0.3.6760",
    "@prague/services": "^0.3.6760",
    "@prague/services-core": "^0.3.6760",
    "@prague/services-utils": "^0.3.6760",
    "@prague/utils": "^0.3.6760",
    "antd": "^3.11.6",
    "body-parser": "^1.18.2",
    "compression": "^1.7.1",
    "concurrently": "^3.5.1",
    "connect-ensure-login": "^0.1.1",
    "connect-redis": "^3.3.3",
    "cookie-parser": "^1.4.3",
    "debug": "^3.1.0",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "hjs": "0.0.6",
    "jquery": "^3.3.1",
    "jsonwebtoken": "^8.5.1",
    "less": "^3.0.4",
    "method-override": "^2.3.10",
    "mongodb": "^2.2.30",
    "moniker": "^0.1.2",
    "morgan": "^1.9.0",
    "nconf": "^0.10.0",
    "passport": "^0.4.0",
    "passport-openidconnect": "0.0.2",
    "performance-now": "^2.1.0",
    "react": "^16.2.0",
    "react-burger-menu": "^2.3.0",
    "react-dom": "^16.2.0",
    "react-iframe": "^1.1.0",
    "react-router-dom": "^4.2.2",
    "redis": "^2.8.0",
    "request": "^2.87.0",
    "request-promise-native": "^1.0.5",
    "serve-favicon": "^2.4.5",
    "split": "^1.0.1",
    "static-expiry": "0.0.11",
    "winston": "^2.4.0"
  },
  "devDependencies": {
    "@types/compression": "0.0.35",
    "@types/connect-ensure-login": "^0.1.4",
    "@types/connect-redis": "0.0.7",
    "@types/cookie-parser": "^1.4.1",
    "@types/debug": "0.0.30",
    "@types/express": "^4.11.0",
    "@types/express-session": "^1.15.6",
    "@types/jquery": "^3.3.0",
    "@types/method-override": "0.0.31",
    "@types/mongodb": "^2.1.42",
    "@types/morgan": "^1.7.35",
    "@types/nconf": "0.0.37",
    "@types/node": "^11.13.8",
    "@types/passport": "^0.4.2",
