  "license": "MIT",
  "author": "Microsoft",
  "main": "dist/www.js",
  "scripts": {
    "build": "concurrently npm:build:compile npm:lint",
    "build:compile": "npm run tsc && npm run less && npm run build:webpack",
    "build:webpack": "npm run webpack",
    "clean": "rimraf dist *.tsbuildinfo *.build.log",
    "docker:build": "docker build -t admin .",
    "docker:start": "docker run --rm -t --network routerlicious_default -p 7000:3000 admin",
    "eslint": "eslint --ext=ts,tsx --format stylish src",
    "eslint:fix": "eslint --ext=ts,tsx --format stylish src --fix",
    "less": "lessc src/stylesheets/style.less public/stylesheets/dist/style.css",
    "lint": "npm run eslint",
    "lint:fix": "npm run eslint:fix",
    "start": "docker-compose up",
    "start:debug": "docker-compose -f docker-compose.yml -f docker-compose.debug.yml up",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tsc": "tsc",
    "webpack": "webpack --config webpack.config.js"
  },
