{
    "logger": {
        "colorize": true,
        "morganFormat": "dev",
        "json": false,
        "level": "info",
        "timestamp": true,
        "label": "winston"
    },
    "notification": {
        "enabled": false,
        "endpoint" : "sentry_endpoint_for_logging_error"
    },
    "loader": {
        "orderer": "<url_for_fluid_orderer>",
        "storage": "<url_for_fluid_storage>",
        "tenant": "<tenant_id>",
        "secret": "<tenant_secret>",
        "jwtKey": "<bearer_jwt_key>",
        "user": "<user_name>",
        "waitMSec": 20000,
        "docId": "<optional_doc_id>",
        "component": {
            "load": false,
            "packageName": "@fluid-example/key-value-cache@0.18.0-25965",
            "installPath": "/tmp/components",
            "timeoutMS": 60000
        }
    }
}
