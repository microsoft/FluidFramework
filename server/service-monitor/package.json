{
  "name": "@fluid-internal/service-monitor",
  "version": "0.0.1",
  "private": true,
  "description": "Node.js script that tests Fluid ordering service",
  "homepage": "https://fluidframework.com",
  "repository": "microsoft/FluidFramework",
  "license": "MIT",
  "author": "Microsoft",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "concurrently npm:build:compile npm:lint",
    "build:compile": "npm run tsc",
    "build:full": "npm run build",
    "build:full:compile": "npm run build:compile",
    "clean": "rimraf dist lib *.tsbuildinfo *.build.log",
    "docker:build": "docker build -t monitoring .",
    "docker:start": "docker run --rm -t monitoring",
    "eslint": "eslint --ext=ts,tsx --format stylish src",
    "eslint:fix": "eslint --ext=ts,tsx --format stylish src --fix",
    "lint": "npm run eslint",
    "start": "node dist/index.js",
    "start:debug": "node --inspect=0.0.0.0:5858 dist/index.js",
    "tsc": "tsc"
  },
  "dependencies": {
    "@fluidframework/container-definitions": "^0.26.0-0",
    "@fluidframework/container-loader": "^0.26.0-0",
    "@fluidframework/driver-definitions": "^0.26.0-0",
    "@fluidframework/routerlicious-driver": "^0.26.0-0",
    "@fluidframework/routerlicious-host": "^0.26.0-0",
    "@fluidframework/server-services": "^0.1012.0-0",
    "@fluidframework/server-services-utils": "^0.1012.0-0",
    "@sentry/node": "^5.6.2",
    "debug": "^4.1.1",
    "jsonwebtoken": "^8.5.1",
    "nconf": "^0.10.0",
    "uuid": "^3.3.2",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@fluidframework/build-common": "^0.19.2-0",
    "@fluidframework/eslint-config-fluid": "^0.19.1-0",
    "@microsoft/api-extractor": "^7.7.2",
    "@types/mocha": "^5.2.5",
    "@typescript-eslint/eslint-plugin": "~2.17.0",
    "@typescript-eslint/eslint-plugin-tslint": "~2.16.0",
    "@typescript-eslint/parser": "~2.17.0",
    "concurrently": "^5.2.0",
    "eslint": "~6.8.0",
    "rimraf": "^2.6.2",
    "tslint": "^5.9.1",
    "typescript": "~3.7.4"
  }
}
