{
  "name": "fluid-service-monitor",
  "version": "0.0.1",
  "description": "Node.js script that connects to a fluid service endpoint",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/FluidFramework.git"
  },
  "license": "MIT",
  "author": "Microsoft",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "build": "concurrently npm:build:compile npm:lint",
    "build:compile": "npm run tsc",
    "build:full": "npm run build",
    "build:full:compile": "npm run build:compile",
    "clean": "rimraf dist lib *.tsbuildinfo *.build.log",
    "docker:build": "docker build -t monitoring .",
    "docker:start": "docker run --rm -t monitoring",
    "eslint": "eslint --ext=ts,tsx --format stylish src",
    "eslint:fix": "eslint --ext=ts,tsx --format stylish src --fix",
    "lint": "npm run eslint",
    "start": "node dist/index.js",
    "start:debug": "node --inspect=0.0.0.0:5858 dist/index.js",
    "tsc": "tsc"
  },
  "dependencies": {
    "@fluidframework/container-definitions": "0.19.0-27956",
    "@fluidframework/container-loader": "0.19.0-27956",
    "@fluidframework/driver-definitions": "0.19.0-27956",
    "@fluidframework/routerlicious-driver": "0.19.0-27956",
    "@fluidframework/routerlicious-host": "0.19.0-27956",
    "@fluidframework/server-services": "0.1006.0-28770",
    "@fluidframework/server-services-utils": "0.1006.0-28770",
    "@sentry/node": "^5.6.2",
    "@types/mocha": "^5.2.5",
    "debug": "^4.1.1",
    "jsonwebtoken": "^8.5.1",
    "nconf": "^0.10.0",
    "uuid": "^3.3.2",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.7.2",
    "@fluidframework/eslint-config-fluid": "0.17.0-27805",
    "@fluidframework/build-common": "0.16.0-27916",
    "@typescript-eslint/eslint-plugin": "~2.17.0",
    "@typescript-eslint/eslint-plugin-tslint": "~2.16.0",
    "@typescript-eslint/parser": "~2.17.0",
    "concurrently": "^4.1.0",
    "eslint": "~6.8.0",
    "rimraf": "^2.6.2",
    "tslint": "^5.9.1",
    "ts-node": "^8.6.2",
    "typescript": "~3.7.4"
  }
}
