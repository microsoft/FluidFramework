{    
    "extends": "@microsoft/fluid-build-common/ts-common-config.json",
    "exclude": [
        "dist",
        "node_modules"
    ],
    "compilerOptions": {
        "target": "es6",
        "strict": true,
        "strictNullChecks": false,
        "moduleResolution": "node",
        "module": "commonjs",
        "rootDir": "./src",
        "outDir": "dist",
        "skipLibCheck": true
    },
    "include": [
        "src/**/*"
    ]
}