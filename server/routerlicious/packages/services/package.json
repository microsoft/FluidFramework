    "@fluidframework/server-services-client": "^0.1014.0",
    "@fluidframework/server-services-core": "^0.1014.0",
    "@fluidframework/server-services-shared": "^0.1014.0",
    "@fluidframework/server-services-utils": "^0.1014.0",
    "amqplib": "^0.5.5",
    "axios": "^0.18.0",
    "debug": "^4.1.1",
    "kafka-node": "^5.0.0",
    "mongodb": "3.1.13",
    "node-rdkafka": "^2.9.0",
