{
  "name": "@fluidframework/server-services-shared",
  "version": "0.1032.1",
  "description": "Fluid server shared services",
  "homepage": "https://fluidframework.com",
  "repository": "https://github.com/microsoft/FluidFramework",
  "license": "MIT",
  "author": "Microsoft and contributors",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build:genver && concurrently npm:build:compile npm:lint",
    "build:compile": "npm run tsc && npm run build:test",
    "build:full": "npm run build",
    "build:full:compile": "npm run build:compile",
    "build:genver": "gen-version",
    "build:test": "tsc --project ./src/test/tsconfig.json",
    "clean": "rimraf dist lib *.tsbuildinfo *.build.log",
    "eslint": "eslint --format stylish src",
    "eslint:fix": "eslint --format stylish src --fix",
    "lint": "npm run eslint",
    "lint:fix": "npm run eslint:fix",
    "test": "mocha --recursive dist/test --unhandled-rejections=strict",
    "test:coverage": "nyc npm test -- --reporter xunit --reporter-option output=nyc/junit-report.xml",
    "tsc": "tsc",
    "tsfmt": "tsfmt --verify",
    "tsfmt:fix": "tsfmt --replace"
  },
  "dependencies": {
    "@fluidframework/common-utils": "^0.32.1",
    "@fluidframework/gitresources": "^0.1032.1",
    "@fluidframework/protocol-base": "^0.1032.1",
    "@fluidframework/protocol-definitions": "^0.1025.0",
    "@fluidframework/server-services-client": "^0.1032.1",
    "@fluidframework/server-services-core": "^0.1032.1",
    "@fluidframework/server-services-telemetry": "^0.1032.1",
    "@socket.io/redis-adapter": "^7.0.0",
    "debug": "^4.1.1",
    "formidable": "2.0.0-canary.20200504.1",
    "ioredis": "^4.24.2",
    "lodash": "^4.17.21",
    "moniker": "^0.1.2",
    "nconf": "^0.11.0",
    "notepack.io": "^2.3.0",
    "socket.io": "^4.1.2",
    "socket.io-adapter": "^2.3.1",
    "socket.io-parser": "^4.0.4",
    "uuid": "^8.3.1",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@fluidframework/build-common": "^0.23.0",
    "@fluidframework/eslint-config-fluid": "^0.23.0",
    "@types/debug": "^4.1.5",
    "@types/formidable": "1.2.3",
    "@types/ioredis": "^4.22.0",
    "@types/lodash": "^4.14.118",
    "@types/mocha": "^8.2.2",
    "@types/nconf": "^0.10.0",
    "@types/node": "^12.19.0",
    "@types/supertest": "^2.0.5",
    "@typescript-eslint/eslint-plugin": "~4.14.0",
    "@typescript-eslint/parser": "~4.14.0",
    "body-parser": "^1.17.1",
    "concurrently": "^5.2.0",
    "eslint": "~7.18.0",
    "eslint-plugin-eslint-comments": "~3.2.0",
    "eslint-plugin-import": "~2.22.1",
    "eslint-plugin-no-null": "~1.0.2",
    "eslint-plugin-prefer-arrow": "~1.2.2",
    "eslint-plugin-react": "~7.22.0",
    "eslint-plugin-unicorn": "~26.0.1",
    "express": "^4.16.3",
    "mocha": "^8.4.0",
    "nyc": "^15.0.0",
    "rimraf": "^2.6.2",
    "supertest": "^3.1.0",
    "typescript": "~4.1.3",
    "typescript-formatter": "7.1.0"
  }
}
