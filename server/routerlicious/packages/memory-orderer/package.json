{
  "name": "@fluidframework/server-memory-orderer",
  "version": "0.1020.0",
  "description": "Fluid server in memory orderer",
  "homepage": "https://fluidframework.com",
  "repository": "https://github.com/microsoft/FluidFramework",
  "license": "MIT",
  "author": "Microsoft",
  "main": "dist/index.js",
  "browser": {
    "moniker": "@fluidframework/server-services-client/dist/generateNames.js"
  },
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "pnpm run build:genver && concurrently pnpm:build:compile pnpm:lint",
    "build:compile": "pnpm run tsc",
    "build:full": "pnpm run build",
    "build:full:compile": "pnpm run build:compile",
    "build:genver": "gen-version",
    "clean": "rimraf dist lib *.tsbuildinfo *.build.log",
    "eslint": "eslint --format stylish src",
    "eslint:fix": "eslint --format stylish src --fix",
    "lint": "pnpm run eslint",
    "lint:fix": "pnpm run eslint:fix",
    "tsc": "tsc",
    "tsfmt": "tsfmt --verify",
    "tsfmt:fix": "tsfmt --replace"
  },
  "dependencies": {
    "@fluidframework/common-utils": "^0.27.0",
    "@fluidframework/protocol-base": "^0.1020.0",
    "@fluidframework/protocol-definitions": "^0.1020.0",
    "@fluidframework/server-lambdas": "^0.1020.0",
    "@fluidframework/server-services-client": "^0.1020.0",
    "@fluidframework/server-services-core": "^0.1020.0",
    "@types/debug": "^4.1.5",
    "@types/double-ended-queue": "^2.1.0",
    "@types/lodash": "^4.14.118",
    "@types/node": "^12.19.0",
    "@types/ws": "^6.0.1",
    "debug": "^4.1.1",
    "double-ended-queue": "^2.1.0-0",
    "lodash": "^4.17.21",
    "moniker": "^0.1.2",
    "uuid": "^8.3.1",
    "ws": "^6.1.2"
  },
  "devDependencies": {
    "@fluidframework/build-common": "^0.20.0-0",
    "@fluidframework/eslint-config-fluid": "^0.23.0-0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "~4.14.0",
    "@typescript-eslint/parser": "~4.14.0",
    "concurrently": "^5.2.0",
    "eslint": "~7.18.0",
    "eslint-plugin-eslint-comments": "~3.2.0",
    "eslint-plugin-import": "~2.22.1",
    "eslint-plugin-no-null": "~1.0.2",
    "eslint-plugin-prefer-arrow": "~1.2.2",
    "eslint-plugin-react": "~7.22.0",
    "eslint-plugin-unicorn": "~26.0.1",
    "rimraf": "^2.6.2",
    "typescript": "~4.1.3",
    "typescript-formatter": "7.1.0"
  }
