{
    "logger": {
        "colorize": true,
        "morganFormat": "json",
        "json": false,
        "level": "info",
        "timestamp": true
    },
    "riddler": "http://riddler:5000",
    "requestSizeLimit": "1gb",
    "redis": {
        "host": "redis",
        "port": 6379
    },
    "redisForThrottling": {
        "host": "redis",
        "port": 6379
    },
    "error": {
        "track": false,
        "endpoint": ""
    },
    "throttling": {
        "maxRequestsPerMs": 1000000,
        "maxRequestBurst": 1000000,
        "minCooldownIntervalInMs": 1000000,
        "minThrottleIntervalInMs": 1000000
    },
    "restGitService": {
        "disableGitCache": false
    },
    "storageUrl": ""
}
