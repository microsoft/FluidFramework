{
  "name": "headless-chrome",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc && tslint 'src/**/*.ts'",
    "build:prague": "webpack --color --config client-src/prague-loader/webpack.config.js",
    "build:full": "npm run build:prague && npm run build",
    "docker:build": "docker build --build-arg NPM_TOKEN=$(echo $NPM_TOKEN) -t headless-agent .",
    "docker:start": "docker run --rm -t --cap-add=SYS_ADMIN --network routerlicious_default headless-agent",
    "start": "docker-compose up",
    "start:debug": "docker-compose -f docker-compose.yml -f docker-compose.debug.yml up"
  },
  "dependencies": {
    "@microsoft/fluid-container-definitions": "^0.3.6078",
    "@microsoft/fluid-container-loader": "^0.3.6078",
    "@microsoft/fluid-routerlicious-host": "^0.3.6078",
    "@microsoft/fluid-routerlicious-driver": "^0.3.6078",
    "@microsoft/fluid-runtime-definitions": "^0.3.6078",
    "@microsoft/fluid-server-services": "^0.3.6078",
    "@microsoft/fluid-server-services-core": "^0.3.6078",
    "@microsoft/fluid-server-services-utils": "^0.3.6078",
    "@microsoft/fluid-core-utils": "^0.3.6078",
    "commander": "^2.15.1",
    "debug": "^4.1.1",
    "jsonwebtoken": "^8.5.0",
    "puppeteer": "^1.14.0",
    "redis": "^2.8.0",
    "winston": "3.2.1"
  },
  "devDependencies": {
    "@types/debug": "^0.0.31",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/mongodb": "^3.1.22",
    "@types/puppeteer": "^1.12.3",
    "@types/redis": "^2.8.10",
    "ts-loader": "^5.3.1",
    "tslint": "^5.10.0",
    "typescript": "~3.4.5",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0"
  },
  "author": "Microsoft",
  "license": "MIT"
}
