  "main": "index.js",
  "scripts": {
    "build": "npm run tsc && npm run lint",
    "build:client": "webpack --color --config client-src/fluid-loader/webpack.config.js",
    "build:full": "npm run build && npm run build:client",
    "docker:build": "docker build --build-arg NPM_TOKEN=$(echo $NPM_TOKEN) -t prague.azurecr.io/headless-agent:local .",
    "docker:local": "docker run --rm -t --cap-add=SYS_ADMIN --network routerlicious_default headless-agent",
    "docker:start": "docker run --rm -t --cap-add=SYS_ADMIN --network routerlicious_default prague.azurecr.io/headless-agent:local",
    "lint": "tslint 'src/**/*.ts'",
    "start": "docker-compose up",
    "start:debug": "docker-compose -f docker-compose.yml -f docker-compose.debug.yml up",
    "test": "node dist/puppeteer/cli.js -u https://www.wu2-ppe.prague.office-int.com -d samasoa5agaz -t search",
    "tsc": "tsc"
  },
  "dependencies": {
