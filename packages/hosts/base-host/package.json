    "concurrently": "^4.1.0",
    "rimraf": "^2.6.2",
    "source-map-loader": "^0.2.4",
    "tslint": "^5.11.0",
    "typescript": "~3.4.5"
  },
  "dependencies": {
    "@prague/component-core-interfaces": "^0.10.0",
    "@prague/container-definitions": "^0.10.0",
    "@prague/container-loader": "^0.10.0",
    "@prague/loader-web": "^0.10.0",
    "@prague/odsp-socket-storage": "^0.10.0",
    "@prague/protocol-definitions": "^0.10.0",
    "@prague/routerlicious-host": "^0.10.0",
    "@prague/routerlicious-socket-storage": "^0.10.0",
    "@prague/iframe-socket-storage": "^0.10.0",
    "@prague/services-client": "^0.10.0"
