  "license": "MIT",
  "author": "Microsoft",
  "sideEffects": "false",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "concurrently npm:build:compile npm:tslint",
    "build:compile": "concurrently npm:tsc npm:build:esnext",
    "build:compile:min": "npm run build:compile",
    "build:docs": "api-extractor run --local",
    "build:esnext": "tsc --project ./tsconfig.esnext.json",
    "build:full": "concurrently npm:build npm:webpack",
    "build:full:compile": "concurrently npm:build:compile npm:webpack",
    "clean": "rimraf dist lib *.tsbuildinfo *.build.log",
    "start": "webpack-dev-server",
    "tsc": "tsc",
    "tslint": "tslint --project tsconfig.json --format verbose",
    "webpack": "webpack  --env build"
  },
  "dependencies": {
    "@microsoft/fluid-base-host": "^0.12.0",
    "@microsoft/fluid-container-definitions": "^0.12.0",
    "@microsoft/fluid-core-utils": "^0.12.0",
    "@microsoft/fluid-driver-definitions": "^0.12.0",
    "@microsoft/fluid-fluidapp-odsp-urlresolver": "^0.12.0",
    "@microsoft/fluid-odsp-driver": "^0.12.0",
    "@microsoft/fluid-odsp-urlresolver": "^0.12.0",
    "@microsoft/fluid-odsp-utils": "^0.12.0",
    "@microsoft/fluid-protocol-definitions": "^0.12.0",
    "@microsoft/fluid-routerlicious-driver": "^0.12.0",
    "@microsoft/fluid-routerlicious-host": "^0.12.0",
    "@microsoft/fluid-routerlicious-urlresolver": "^0.12.0",
    "@microsoft/fluid-container-loader": "^0.12.0",
    "@microsoft/fluid-web-code-loader": "^0.12.0",
    "uuid": "^3.3.2",
    "jsonwebtoken": "^8.4.0"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.4.4",
    "@microsoft/fluid-build-common": "^0.12.0",
    "@types/node": "^10.12.12",
    "concurrently": "^4.1.0",
    "rimraf": "^2.6.2",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^4.5.0",
    "tslint": "^5.11.0",
    "typescript": "~3.4.5",
    "webpack": "^4.8.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.8.0"
  }
}
