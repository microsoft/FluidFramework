{
  "name": "@prague/tiny-web-host",
  "version": "0.9.0",
  "description": "Loader client app",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": "false",
  "scripts": {
    "build": "concurrently npm:build:compile npm:tslint",
    "build:compile": "concurrently npm:tsc npm:build:esnext",
    "build:full": "npm run build",
    "build:webpack": "webpack --env build",
    "build:full:compile": "npm run build:compile",
    "build:docs": "api-extractor run --local",
    "start": "webpack-dev-server",
    "clean": "rimraf dist lib *.tsbuildinfo package-deps.json *.build.log",
    "tsc": "tsc",
    "build:esnext": "tsc --project ./tsconfig.esnext.json",
    "tslint": "tslint --project tsconfig.json --format verbose",
    "webpack": "webpack  --env build"
  },
  "author": "Microsoft",
  "repository": "microsoft/prague",
  "license": "MIT",
  "dependencies": {
    "@prague/base-host": "^0.9.0",
    "@prague/loader-web": "^0.9.0",
    "@prague/odsp-utils": "^0.9.0",
    "@prague/protocol-definitions": "^0.9.0",
    "@prague/services-core": "^0.9.0",
    "@prague/routerlicious-host": "^0.9.0",
    "@prague/routerlicious-socket-storage": "^0.9.0",
    "jsonwebtoken": "^8.4.0",
    "url-parse": "^1.4.3"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.1.6",
    "@prague/build-common": "^0.9.0",
    "@types/node": "^10.12.12",
    "@types/url-parse": "^1.4.1",
    "concurrently": "^4.1.0",
    "rimraf": "^2.6.2",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^4.5.0",
    "tslint": "^5.11.0",
    "typescript": "~3.4.5",
    "webpack": "^4.8.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.14"
  }
}
