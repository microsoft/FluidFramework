{
  "name": "@microsoft/fluid-odsp-utils",
  "version": "0.10.0",
  "description": "ODSP utilities",
  "module": "lib/index.js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "concurrently npm:build:compile npm:tslint",
    "build:compile": "concurrently npm:tsc npm:build:esnext",
    "build:esnext": "tsc --project ./tsconfig.esnext.json",
    "build:full": "npm run build",
    "build:full:compile": "npm run build:compile",
    "clean": "rimraf dist *.tsbuildinfo package-deps.json *.build.log",
    "tslint": "tslint --project tsconfig.json --format verbose",
    "tsc": "tsc"
  },
  "author": "Microsoft",
  "repository": "microsoft/FluidFramework",
  "license": "MIT",
  "dependencies": {
    "request": "^2.88.0"
  },
  "devDependencies": {
    "@microsoft/fluid-build-common": "^0.10.0",
    "@types/request": "^2.47.1",
    "concurrently": "^4.1.0",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "typescript": "~3.4.5"
  }
}
