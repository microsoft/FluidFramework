{
  "name": "@fluid-internal/client-api",
  "version": "0.12.0",
  "description": "Fluid client API",
  "repository": "microsoft/FluidFramework",
  "license": "MIT",
  "author": "Microsoft",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "concurrently npm:build:compile npm:tslint",
    "build:compile": "concurrently npm:tsc npm:build:esnext",
    "build:docs": "api-extractor run --local",
    "build:esnext": "tsc --project ./tsconfig.esnext.json",
    "build:full": "npm run build",
    "build:full:compile": "npm run build:compile",
    "clean": "rimraf dist lib *.tsbuildinfo package-deps.json *.build.log",
    "tsc": "tsc",
    "tslint": "tslint --project tsconfig.json --format verbose"
  },
  "dependencies": {
    "@microsoft/fluid-cell": "^0.12.0",
    "@microsoft/fluid-component-core-interfaces": "^0.12.0",
    "@microsoft/fluid-component-runtime": "^0.12.0",
    "@microsoft/fluid-container-definitions": "^0.12.0",
    "@microsoft/fluid-container-loader": "^0.12.0",
    "@microsoft/fluid-container-runtime": "^0.12.0",
    "@microsoft/fluid-core-utils": "^0.12.0",
    "@microsoft/fluid-driver-definitions": "^0.12.0",
    "@microsoft/fluid-ink": "^0.12.0",
    "@microsoft/fluid-map": "^0.12.0",
    "@microsoft/fluid-ordered-collection": "^0.12.0",
    "@microsoft/fluid-protocol-definitions": "^0.12.0",
    "@microsoft/fluid-register-collection": "^0.12.0",
    "@microsoft/fluid-runtime-definitions": "^0.12.0",
    "@microsoft/fluid-sequence": "^0.12.0",
    "@microsoft/fluid-shared-object-base": "^0.12.0",
    "@types/node": "^10.12.12",
    "debug": "^4.1.1",
    "jwt-decode": "^2.2.0"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.4.4",
    "@microsoft/fluid-build-common": "^0.12.0",
    "@types/debug": "^0.0.31",
    "@types/jwt-decode": "^2.2.1",
    "@types/mocha": "^5.2.5",
    "concurrently": "^4.1.0",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.18.0",
    "nyc": "^14.1.1",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "typescript": "~3.4.5",
    "xmldoc": "^1.1.2"
  },
  "nyc": {
    "all": true,
    "exclude": [
      "src/test/**/*.ts",
      "dist/test/**/*.js"
    ],
    "include": [
      "src/**/*.ts",
      "dist/**/*.js"
    ],
    "reporter": [
      "cobertura",
      "html",
      "text"
    ],
    "temp-directory": "nyc/.nyc_output",
    "cache-dir": "nyc/.cache",
    "report-dir": "nyc/report"
  }
}
