{
  "name": "@prague/client-api",
  "version": "0.10.0",
  "description": "Prague client API",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "concurrently npm:build:compile npm:tslint",
    "build:compile": "concurrently npm:tsc npm:build:esnext",
    "build:full": "npm run build",
    "build:full:compile": "npm run build:compile",
    "build:docs": "api-extractor run --local",
    "clean": "rimraf dist lib *.tsbuildinfo package-deps.json *.build.log",
    "tsc": "tsc",
    "build:esnext": "tsc --project ./tsconfig.esnext.json",
    "tslint": "tslint --project tsconfig.json --format verbose"
  },
  "author": "Microsoft",
  "repository": "microsoft/prague",
  "nyc": {
    "all": true,
    "exclude": [
      "src/test/**/*.ts",
      "dist/test/**/*.js"
    ],
    "include": [
      "src/**/*.ts",
      "dist/**/*.js"
    ],
    "reporter": [
      "cobertura",
      "html",
      "text"
    ],
    "temp-directory": "nyc/.nyc_output",
    "cache-dir": "nyc/.cache",
    "report-dir": "nyc/report"
  },
  "dependencies": {
    "@microsoft/fluid-cell": "^0.10.0",
    "@prague/component-core-interfaces": "^0.10.0",
    "@microsoft/fluid-component-runtime": "^0.10.0",
    "@microsoft/fluid-ordered-collection": "^0.10.0",
    "@microsoft/fluid-register-collection": "^0.10.0",
    "@prague/container-definitions": "^0.10.0",
    "@prague/container-loader": "^0.10.0",
    "@microsoft/fluid-container-runtime": "^0.10.0",
    "@microsoft/fluid-ink-stream": "^0.10.0",
    "@microsoft/fluid-map": "^0.10.0",
    "@prague/protocol-definitions": "^0.10.0",
    "@microsoft/fluid-runtime-definitions": "^0.10.0",
    "@microsoft/fluid-sequence": "^0.10.0",
    "@microsoft/fluid-shared-object-base": "^0.10.0",
    "@prague/utils": "^0.10.0",
    "@types/node": "^10.12.12",
    "debug": "^4.1.1",
    "jwt-decode": "^2.2.0"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.3.11",
    "@microsoft/fluid-build-common": "^0.10.0",
    "@types/debug": "^0.0.31",
    "@types/jwt-decode": "^2.2.1",
    "@types/mocha": "^5.2.5",
    "concurrently": "^4.1.0",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.18.0",
    "nyc": "^14.1.1",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "typescript": "~3.4.5",
    "xmldoc": "^1.1.2"
  },
  "license": "MIT"
}
