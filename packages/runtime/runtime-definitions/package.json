{
  "name": "@prague/runtime-definitions",
  "version": "0.8.0",
  "description": "Prague Runtime definitions",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": "false",
  "scripts": {
    "build": "concurrently npm:build:compile npm:tslint",
