    "tslint": "^5.11.0",
    "typescript": "~3.4.5"
  },
  "dependencies": {
    "@prague/container-definitions": "^0.8.0",
    "@prague/gitresources": "^0.8.0",
    "@prague/utils": "^0.8.0",
    "@types/node": "^10.12.12"
  },
  "license": "MIT"
}
