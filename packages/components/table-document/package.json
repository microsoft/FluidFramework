{
  "name": "@fluid-example/table-document",
  "version": "0.13.0",
  "description": "Chaincode component containing a table's data",
  "repository": "microsoft/FluidFramework",
  "license": "MIT",
  "author": "Microsoft",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "concurrently npm:build:compile npm:tslint",
    "build:compile": "concurrently npm:tsc npm:build:esnext",
    "build:compile:min": "npm run build:compile",
    "build:esnext": "tsc --project ./tsconfig.esnext.json",
    "build:full": "npm run build",
    "build:full:compile": "npm run build:compile",
    "clean": "rimraf dist lib *.tsbuildinfo *.build.log",
    "eslint": "eslint --ext=ts,tsx --format stylish src",
    "eslint:fix": "eslint --ext=ts,tsx --format stylish src --fix",
    "test": "mocha --recursive test/**/*.spec.ts --exit -r ts-node/register -r make-promises-safe",
    "test:coverage": "nyc npm test -- --reporter mocha-junit-reporter --reporter-options mochaFile=nyc/junit-report.xml --exit",
    "tsc": "tsc",
    "tslint": "npm run eslint"
  },
  "dependencies": {
    "@microsoft/fluid-aqueduct": "^0.13.0",
    "@microsoft/fluid-component-core-interfaces": "^0.13.0",
    "@microsoft/fluid-merge-tree": "^0.13.0",
    "@microsoft/fluid-runtime-definitions": "^0.13.0",
    "@microsoft/fluid-sequence": "^0.13.0",
    "@tiny-calc/micro": "0.0.18374",
    "debug": "^4.1.1"
  },
  "devDependencies": {
    "@microsoft/eslint-config-fluid": "^0.13.0",
    "@microsoft/fluid-build-common": "^0.13.0",
    "@microsoft/fluid-local-test-server": "^0.13.0",
    "@types/debug": "^0.0.31",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.14.6",
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "concurrently": "^4.1.0",
    "eslint": "^6.7.1",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-optimize-regex": "^1.1.7",
    "eslint-plugin-prefer-arrow": "^1.1.7",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-unicorn": "^14.0.1",
    "make-promises-safe": "^5.1.0",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.18.0",
    "nyc": "^14.1.1",
    "rimraf": "^2.6.2",
    "ts-node": "^7.0.1",
    "typescript": "~3.4.5"
  },
  "nyc": {
    "all": true,
    "cache-dir": "nyc/.cache",
    "exclude-after-remap": false,
    "include": [
      "src/**/*.ts",
      "dist/**/*.js"
    ],
    "report-dir": "nyc/report",
    "reporter": [
      "cobertura",
      "html",
      "text"
    ],
    "temp-directory": "nyc/.nyc_output"
  },
  "fluid": {
    "browser": {
      "umd": {
        "files": [
          "dist/main.bundle.js"
        ],
        "library": "main"
      }
    }
  }
}
