{
  "name": "@chaincode/shared-text",
  "version": "0.10.0",
  "description": "Shared text",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": [
    "./src/publicpath.ts"
  ],
  "scripts": {
    "build": "npm run build:compile",
    "build:compile": "concurrently npm:tsc npm:build:esnext npm:webpack:dev",
    "build:full": "npm run build",
    "build:full:compile": "npm run build:compile && npm run webpack",
    "clean": "rimraf dist lib *.tsbuildinfo package-deps.json *.build.log",
    "tsc": "tsc",
    "build:esnext": "tsc --project ./tsconfig.esnext.json",
    "tslint": "tslint --project tsconfig.json --format verbose",
    "start": "webpack-dev-server --config webpack.config.js --package package.json",
    "start:localhost": "webpack-dev-server --config webpack.config.js --package package.json --env.mode  localhost",
    "start:local": "webpack-dev-server --config webpack.config.js --package package.json --env.mode  local",
    "webpack": "webpack --env.production",
    "webpack:dev": "webpack --env.development"
  },
  "author": "Microsoft",
  "repository": "microsoft/prague",
  "prague": {
    "browser": {
      "entrypoint": "main",
      "bundle": [
        "dist/main.bundle.js"
      ]
    }
  },
  "fluid": {
    "browser": {
      "umd": {
        "files": [
          "dist/main.bundle.js"
        ],
        "library": "main"
      }
    }
  },
  "dependencies": {
    "@fluid-example/image-collection": "^0.10.0",
    "@fluid-example/math": "^0.10.0",
    "@fluid-example/monaco": "^0.10.0",
    "@fluid-example/pinpoint-editor": "^0.10.0",
    "@fluid-example/progress-bars": "^0.10.0",
    "@fluid-example/video-players": "^0.10.0",
    "@prague/cell": "^0.10.0",
    "@prague/client-api": "^0.10.0",
    "@prague/client-ui": "^0.10.0",
    "@prague/component-core-interfaces": "^0.10.0",
    "@prague/component-runtime": "^0.10.0",
    "@prague/container-definitions": "^0.10.0",
    "@prague/container-runtime": "^0.10.0",
    "@prague/ink": "^0.10.0",
    "@prague/intelligence-runner": "^0.10.0",
    "@prague/map": "^0.10.0",
    "@prague/merge-tree": "^0.10.0",
    "@prague/runtime-definitions": "^0.10.0",
    "@prague/sequence": "^0.10.0",
    "@prague/snapshotter": "^0.10.0",
    "@types/graphql": "^14.0.7",
    "@types/node": "^10.12.12",
    "@types/react": "^16.7.7",
    "axios": "^0.18.0",
    "bootstrap": "^3.3.7",
    "debug": "^4.1.1",
    "graphiql": "^0.13.0",
    "graphql": "^14.1.1",
    "graphql-subscriptions": "^1.0.0",
    "katex": "^0.10.2",
    "monaco-editor": "^0.15.6",
    "performance-now": "^2.1.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3"
  },
  "devDependencies": {
    "@microsoft/fluid-webpack-component-loader": "^0.10.0",
    "@microsoft/fluid-build-common": "^0.10.0",
    "@types/debug": "^0.0.31",
    "concurrently": "^4.1.0",
    "css-loader": "^1.0.0",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "loader-utils": "^1.1.0",
    "monaco-editor-webpack-plugin": "^1.5.4",
    "node-sass": "^4.9.3",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.0",
    "ts-loader": "^4.5.0",
    "tslint": "^5.11.0",
    "typescript": "~3.4.5",
    "url-loader": "^1.1.1",
    "webpack": "^4.8.2",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.1.2",
    "webpack-visualizer-plugin": "^0.1.11"
  },
  "license": "MIT"
}
