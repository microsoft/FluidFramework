{
  "name": "@fluid-example/source-document",
  "version": "0.12.0",
  "description": "Experiments with incremental parsing.",
  "repository": "microsoft/FluidFramework",
  "license": "MIT",
  "author": "Microsoft",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "concurrently npm:build:compile npm:tslint",
    "build:compile": "concurrently npm:tsc npm:build:esnext",
    "build:compile:min": "npm run build:compile",
    "build:esnext": "tsc --project ./tsconfig.esnext.json",
    "build:full": "npm run build",
    "build:full:compile": "npm run build:compile",
    "clean": "rimraf dist lib *.tsbuildinfo *.build.log",
    "dev": "npm run build:esnext -- --watch",
    "test": "mocha -r ts-node/register -r ignore-styles --recursive test/**/*.spec.ts --exit",
    "test:coverage": "nyc npm test -- --reporter mocha-junit-reporter --reporter-options mochaFile=nyc/junit-report.xml --exit",
    "tsc": "tsc",
    "tslint": "tslint --project tsconfig.json --format verbose"
  },
  "dependencies": {
    "@microsoft/fluid-aqueduct": "^0.12.0",
    "@microsoft/fluid-component-core-interfaces": "^0.12.0",
    "@microsoft/fluid-merge-tree": "^0.12.0",
    "@microsoft/fluid-runtime-definitions": "^0.12.0",
    "@microsoft/fluid-sequence": "^0.12.0",
    "debug": "^4.1.1"
  },
  "devDependencies": {
    "@microsoft/fluid-build-common": "^0.12.0",
    "@microsoft/fluid-local-test-server": "^0.12.0",
    "@types/debug": "^0.0.31",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.14.6",
    "concurrently": "^4.1.0",
    "copyfiles": "^2.1.0",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.18.0",
    "nyc": "^14.1.1",
    "rimraf": "^2.6.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.20.0",
    "typescript": "~3.7.2",
    "url-loader": "^2.1.0"
  },
  "fluid": {
    "browser": {
      "umd": {
        "files": [
          "dist/main.bundle.js"
        ],
        "library": "main"
      }
    }
  }
}
