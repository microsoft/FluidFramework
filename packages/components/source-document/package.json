{
  "name": "@fluid-example/source-document",
  "version": "0.11.0",
  "description": "Experiments with incremental parsing.",
  "repository": "microsoft/FluidFramework",
  "license": "MIT",
  "author": "Microsoft",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build:compile",
    "build:compile": "concurrently npm:tsc npm:build:esnext",
    "build:esnext": "tsc --project ./tsconfig.esnext.json",
    "build:full": "npm run build",
    "build:full:compile": "npm run build:compile",
    "clean": "rimraf dist lib *.tsbuildinfo package-deps.json *.build.log",
    "dev": "npm run build:esnext -- --watch",
    "eslint": "eslint --ext=ts,tsx --format codeframe src",
    "test": "mocha -r ts-node/register -r ignore-styles --recursive test/**/*.spec.ts --exit",
    "test:coverage": "nyc npm test -- --reporter mocha-junit-reporter --reporter-options mochaFile=nyc/junit-report.xml --exit",
    "tsc": "tsc",
    "tslint": "tslint --project tsconfig.json --format verbose"
  },
  "dependencies": {
    "@microsoft/fluid-aqueduct": "^0.11.0",
    "@microsoft/fluid-component-core-interfaces": "^0.11.0",
    "@microsoft/fluid-map": "^0.11.0",
    "@microsoft/fluid-merge-tree": "^0.11.0",
    "@microsoft/fluid-runtime-definitions": "^0.11.0",
    "@microsoft/fluid-sequence": "^0.11.0",
    "@tiny/types": "^0.0.12772",
    "debug": "^4.1.1"
  },
  "devDependencies": {
    "@microsoft/eslint-config-fluid": "^0.11.0",
    "@microsoft/fluid-build-common": "^0.11.0",
    "@microsoft/fluid-local-test-server": "^0.11.0",
    "@types/debug": "^0.0.31",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.12",
    "@typescript-eslint/eslint-plugin": "^2.4.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.4.0",
    "@typescript-eslint/parser": "^2.4.0",
    "concurrently": "^4.1.0",
    "copyfiles": "^2.1.0",
    "eslint": "^6.5.1",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.18.0",
    "nyc": "^14.1.1",
    "rimraf": "^2.6.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "~3.4.5",
    "url-loader": "^1.1.1"
  },
  "fluid": {
    "browser": {
      "umd": {
        "files": [
          "dist/main.bundle.js"
        ],
        "library": "main"
      }
    }
  }
}
