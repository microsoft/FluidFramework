{
  "name": "@chaincode/math",
  "version": "0.10.0",
  "description": "Chaincode component that provides math.",
  "main": "dist/mathComponent.js",
  "module": "lib/mathComponent.js",
  "sideEffects": false,
  "types": "dist/mathComponent.d.ts",
  "prague": {
    "browser": {
      "entrypoint": "main",
      "bundle": [
        "dist/main.bundle.js"
      ]
    }
  },
  "fluid": {
    "browser": {
      "umd": {
        "files": [
          "dist/main.bundle.js"
        ],
        "library": "main"
      }
    }
  },
  "scripts": {
    "build": "concurrently npm:build:compile npm:tslint",
    "build:compile": "concurrently npm:tsc npm:build:esnext npm:build:copy",
    "build:copy": "copyfiles -u 1 \"src/**/*.css\" dist/ && copyfiles -u 1 \"src/**/*.css\" lib/",
    "build:esnext": "tsc --project ./tsconfig.esnext.json",
    "build:full": "npm run build",
    "build:full:compile": "npm run build:compile",
    "clean": "rimraf dist lib *.tsbuildinfo package-deps.json *.build.log",
    "tsc": "tsc",
    "tslint": "tslint --project tsconfig.json --format verbose",
    "deploy": "npm publish --registry https://packages.wu2.prague.office-int.com",
    "webpack": "webpack --env=\"production\"",
    "webpack:dev": "webpack --env=\"development\""
  },
  "author": "Microsoft",
  "repository": "microsoft/prague",
  "dependencies": {
    "@chaincode/search-menu": "^0.10.0",
    "@prague/client-ui": "^0.10.0",
    "@prague/component-core-interfaces": "^0.10.0",
    "@prague/component-runtime": "^0.10.0",
    "@prague/flow-util": "^0.10.0",
    "@prague/map": "^0.10.0",
    "@prague/merge-tree": "^0.10.0",
    "@prague/runtime-definitions": "^0.10.0",
    "@prague/sequence": "^0.10.0",
    "@prague/shared-object-common": "^0.10.0",
    "@prague/framework-definitions": "^0.10.0",
    "debug": "^4.1.1",
    "katex": "^0.10.2"
  },
  "devDependencies": {
    "@microsoft/fluid-build-common": "^0.10.0",
    "@types/node": "^10.12.12",
    "concurrently": "^4.1.0",
    "copyfiles": "^2.1.0",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "typescript": "~3.4.5",
    "webpack": "^4.8.2",
    "webpack-cli": "^3.1.2",
    "webpack-merge": "^4.1.2"
  },
  "license": "MIT"
}
