{
  "name": "@fluid-example/pinpoint-editor",
  "version": "0.11.0",
  "description": "A web app for creating and editing Pinpoint maps.",
  "license": "MIT",
  "author": "Microsoft",
  "sideEffects": "false",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "concurrently npm:build:compile npm:tslint",
    "build:compile": "concurrently npm:tsc npm:build:esnext",
    "build:esnext": "tsc --project ./tsconfig.esnext.json",
    "build:full": "npm run build",
    "build:full:compile": "npm run build:compile && npm run webpack",
    "clean": "rimraf dist lib *.tsbuildinfo package-deps.json *.build.log",
    "start": "webpack-dev-server --config webpack.config.js --package package.json --env.component --env.mode local",
    "start:live": "webpack-dev-server --config webpack.config.js --package package.json --env.component --env.mode live",
    "start:localhost": "webpack-dev-server --config webpack.config.js --package package.json --env.component --env.mode localhost",
    "tsc": "tsc",
    "tslint": "tslint --project tsconfig.json --format verbose",
    "webpack": "webpack --config webpack.config.js"
  },
  "dependencies": {
    "@kurtb/pinpoint": "^0.1.2",
    "@microsoft/fluid-component-core-interfaces": "^0.11.0",
    "@microsoft/fluid-component-runtime": "^0.11.0",
    "@microsoft/fluid-framework-interfaces": "^0.11.0",
    "@microsoft/fluid-map": "^0.11.0",
    "@microsoft/fluid-runtime-definitions": "^0.11.0",
    "@types/angular": "^1.6.50",
    "@types/angular-route": "^1.7.0",
    "@types/google-maps": "^3.2.0",
    "@types/jquery": "^3.3.6",
    "@types/leaflet": "^1.2.11",
    "@types/node": "^10.12.12",
    "angular": "^1.7.4",
    "angular-route": "^1.7.4",
    "bootstrap": "^3.3.7",
    "google-maps": "^3.3.0",
    "jquery": "^3.3.1",
    "leaflet": "^1.3.4"
  },
  "devDependencies": {
    "@microsoft/fluid-build-common": "^0.11.0",
    "@microsoft/fluid-webpack-component-loader": "^0.11.0",
    "concurrently": "^4.1.0",
    "css-loader": "^1.0.0",
    "html-loader": "^0.5.5",
    "node-sass": "^4.9.3",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.0",
    "ts-loader": "^4.5.0",
    "tslint": "^5.11.0",
    "typescript": "~3.4.5",
    "url-loader": "^1.1.1",
    "webpack": "^4.8.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.8.0",
    "webpack-serve": "^2.0.2"
  },
  "fluid": {
    "browser": {
      "umd": {
        "files": [
          "dist/main.bundle.js"
        ],
        "library": "main"
      }
    }
  }
}
