{
  "name": "@chaincode/pinpoint-editor",
  "version": "0.3.1",
  "description": "A web app for creating and editing Pinpoint maps.",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": "false",
  "author": "Microsoft",
  "scripts": {
    "build": "concurrently npm:build:compile npm:tslint",
    "build:compile": "concurrently npm:tsc npm:build:esnext",
    "build:full": "npm run build",
    "build:full:compile": "npm run build:compile && npm run webpack",
    "clean": "rimraf dist *.tsbuildinfo package-deps.json *.build.log",
    "tsc": "tsc",
    "build:esnext": "tsc --project ./tsconfig.esnext.json",
    "tslint": "tslint --project tsconfig.json --format verbose",
    "start": "prague-dev-server --config webpack.config.js --package package.json --component --live",
    "start:local": "prague-dev-server --config webpack.config.js --package package.json --component",
    "webpack": "webpack --config webpack.config.js"
  },
  "dependencies": {
    "@kurtb/pinpoint": "^0.1.2",
    "@prague/client-api": "^0.3.0",
    "@prague/map": "^0.3.0",
    "@prague/component-runtime": "^0.3.0",
    "@prague/container-definitions": "^0.3.0",
    "@prague/runtime-definitions": "^0.3.0",
    "@prague/utils": "^0.3.0",
    "@types/angular": "^1.6.50",
    "@types/angular-route": "^1.7.0",
    "@types/debug": "^0.0.31",
    "@types/google-maps": "^3.2.0",
    "@types/jquery": "^3.3.6",
    "@types/leaflet": "^1.2.11",
    "@types/node": "^10.12.12",
    "angular": "^1.7.4",
    "angular-route": "^1.7.4",
    "bootstrap": "^3.3.7",
    "debug": "^3.1.0",
    "file-loader": "^2.0.0",
    "google-maps": "^3.3.0",
    "jquery": "^3.3.1",
    "leaflet": "^1.3.4"
  },
  "devDependencies": {
    "@prague/build-common": "^0.3.0",
    "@prague/prague-dev-server": "^3.3.8",
    "concurrently": "^4.1.0",
    "css-loader": "^1.0.0",
    "html-loader": "^0.5.5",
    "node-sass": "^4.9.3",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^0.23.0",
    "ts-loader": "^4.5.0",
    "tslint": "^5.11.0",
    "typescript": "^3.4.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.8.2",
    "webpack-cli": "^3.1.2",
    "webpack-serve": "^2.0.2"
  },
  "prague": {
    "browser": {
      "entrypoint": "main",
      "bundle": [
        "dist/main.bundle.js"
      ]
    }
  }
}
