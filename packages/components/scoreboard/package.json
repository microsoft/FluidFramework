{
  "name": "@fluid-example/scoreboard",
  "description": "Scoreboard component",
  "version": "0.10.0",
  "author": "Microsoft",
  "repository": "microsoft/prague",
  "license": "MIT",
  "main": "dist/main.bundle.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "webpack --env.production",
    "dev": "webpack --env.development",
    "clean": "rimraf dist *.tsbuildinfo package-deps.json *.build.log",
    "start": "webpack-dev-server --config webpack.config.js --package package.json",
    "start:localhost": "webpack-dev-server --config webpack.config.js --package package.json --env.mode  localhost",
    "start:local": "webpack-dev-server --config webpack.config.js --package package.json --env.mode  local",
    "verdaccio": "npm publish --registry https://packages.wu2.prague.office-int.com",
    "deploy": "npm run build && npm version patch && npm run verdaccio && npm run post-deploy",
    "post-deploy": "getUrl -p",
    "webpack": "webpack --env.production",
    "webpack:dev": "webpack --env.development"
  },
  "prague": {
    "browser": {
      "entrypoint": "main",
      "bundle": [
        "dist/main.bundle.js"
      ]
    }
  },
  "dependencies": {
    "@prague/aqueduct": "^0.10.0",
    "@prague/component-core-interfaces": "^0.10.0",
    "@microsoft/fluid-map": "^0.10.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.9.0",
    "@microsoft/fluid-webpack-component-loader": "^0.10.0",
    "@microsoft/fluid-build-common": "^0.10.0",
    "@prague/url-generator": "^1.0.4",
    "@types/node": "^10.12.12",
    "@types/react": "^16.7.7",
    "@types/react-dom": "^16.0.11",
    "bulma": "^0.7.5",
    "css-loader": "^1.0.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.9.3",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "ts-loader": "^4.5.0",
    "tslint": "^5.11.0",
    "typescript": "~3.4.5",
    "webpack": "^4.8.2",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.8.0",
    "webpack-merge": "^4.1.2"
  }
}
