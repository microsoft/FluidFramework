{
  "name": "@fluid-example/table-test-lib",
  "version": "0.11.0",
  "description": "Test application that loads TableDocument component in node environment",
  "repository": "microsoft/FluidFramework",
  "license": "MIT",
  "author": "Microsoft",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:compile": "npm run build",
    "build:full:compile": "npm run build",
    "clean": "rimraf dist *.tsbuildinfo package-deps.json *.build.log",
    "test": "mocha --recursive dist/**/*.spec.js --exit",
    "test:coverage": "nyc npm test -- --reporter mocha-junit-reporter --reporter-options mochaFile=nyc/junit-report.xml --exit"
  },
  "dependencies": {
    "@fluid-example/table-document": "^0.11.0",
    "@microsoft/fluid-local-test-server": "^0.11.0",
    "@microsoft/fluid-sequence": "^0.11.0",
    "mocha": "^5.2.0"
  },
  "devDependencies": {
    "@microsoft/fluid-build-common": "^0.11.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.12",
    "mocha-junit-reporter": "^1.18.0",
    "nyc": "^14.1.1",
    "rimraf": "^2.6.2",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^4.5.0",
    "tslint": "^5.11.0",
    "typescript": "~3.4.5"
  },
  "nyc": {
    "all": true,
    "exclude": [
      "src/test/**/*.ts",
      "dist/test/**/*.js"
    ],
    "include": [
      "src/**/*.ts",
      "dist/**/*.js"
    ],
    "reporter": [
      "cobertura",
      "html",
      "text"
    ],
    "temp-directory": "nyc/.nyc_output",
    "cache-dir": "nyc/.cache",
    "report-dir": "nyc/report"
  }
}
