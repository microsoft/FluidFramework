{
  "name": "@prague/table-test",
  "description": "Test application that loads TableDocument component in node environment",
  "version": "0.5.0",
  "author": "Microsoft",
  "repository": "microsoft/prague",
  "main": "dist/main.bundle.js",
  "module": "lib/index.js",
  "sideEffects": false,
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:compile": "npm run build",
    "build:full:compile": "npm run build",
    "clean": "rimraf dist *.tsbuildinfo package-deps.json *.build.log",
    "test": "mocha --recursive dist/**/*.spec.js --exit",
    "test:coverage": "nyc npm test -- --reporter mocha-junit-reporter --reporter-options mochaFile=nyc/junit-report.xml --exit"
  },
  "nyc": {
    "all": true,
    "exclude": [
      "src/test/**/*.ts",
      "dist/test/**/*.js"
    ],
    "include": [
      "src/**/*.ts",
      "dist/**/*.js"
    ],
    "reporter": [
      "cobertura",
      "html",
      "text"
    ],
    "temp-directory": "nyc/.nyc_output",
    "cache-dir": "nyc/.cache",
    "report-dir": "nyc/report"
  },
  "dependencies": {
    "@chaincode/table-document": "^0.5.0",
    "@prague/app-component": "^0.5.0",
    "@prague/app-datastore": "^0.5.0",
    "@prague/container-definitions": "^0.5.0",
    "@prague/local-test-server": "^0.5.0",
    "@prague/routerlicious-socket-storage": "^0.5.0",
    "@prague/sequence": "^0.5.0",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.18.0",
    "nyc": "^13.0.1"
  },
  "devDependencies": {
    "@prague/build-common": "^0.5.0",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.12",
    "rimraf": "^2.6.2",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^4.5.0",
    "tslint": "^5.11.0",
    "tslint-microsoft-contrib": "^6.0.0",
    "typescript": "~3.4.5"
  }
}
