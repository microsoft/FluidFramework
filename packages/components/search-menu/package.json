{
  "name": "@chaincode/search-menu",
  "description": "Search menu",
  "version": "0.9.0",
  "author": "Microsoft",
  "repository": "microsoft/prague",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "concurrently npm:build:compile npm:tslint",
    "build:compile": "concurrently npm:tsc npm:build:esnext",
    "build:full": "npm run build",
    "build:full:compile": "npm run build:compile",
    "clean": "rimraf dist lib *.tsbuildinfo package-deps.json *.build.log",
    "tsc": "tsc",
    "build:esnext": "tsc --project ./tsconfig.esnext.json",
    "tslint": "tslint --project tsconfig.json --format verbose"
  },
  "prague": {
    "browser": {
      "entrypoint": "main",
      "bundle": [
        "dist/main.bundle.js"
      ]
    }
  },
  "fluid": {
    "browser": {
      "umd": {
        "files": [
          "dist/main.bundle.js"
        ],
        "library": "main"
      }
    }
  },
  "dependencies": {
    "@prague/component-core-interfaces": "^0.9.0",
    "@prague/merge-tree": "^0.9.0",
    "@types/node": "^10.12.12"
  },
  "devDependencies": {
    "@prague/build-common": "^0.9.0",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "typescript": "~3.4.5"
  },
  "license": "MIT"
}
