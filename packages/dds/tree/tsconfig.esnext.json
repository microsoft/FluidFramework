{
	"extends": "./tsconfig.base.json",
	"compilerOptions": {
		"outDir": "./lib",
		// Node16 and ESNext is effectively the same for transpile so long as package.json has
		// "type": "module", which is required to activate full ESM Mode within tsc.
		"module": "Node16",
		"moduleResolution": "Node16",
	},
}
