{
  "name": "@microsoft/fluid-iframe-driver",
  "version": "0.12.0",
  "description": "Connection Fluid service API that crosses iframe boundary",
  "repository": "microsoft/FluidFramework",
  "license": "MIT",
  "author": "Microsoft",
  "sideEffects": "false",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build:genver && concurrently npm:build:compile npm:lint",
    "build:compile": "concurrently npm:tsc npm:build:esnext",
    "build:compile:min": "npm run build:compile",
    "build:docs": "api-extractor run --local",
    "build:esnext": "tsc --project ./tsconfig.esnext.json",
    "build:full": "npm run build",
    "build:full:compile": "npm run build:compile",
    "build:genver": "node node_modules/@microsoft/fluid-build-common/gen_version.js",
    "clean": "rimraf dist lib *.tsbuildinfo *.build.log",
    "eslint": "eslint --ext=ts,tsx --format stylish src",
    "eslint:fix": "eslint --ext=ts,tsx --format stylish src --fix",
    "tsc": "tsc",
    "lint": "tslint --project tsconfig.json --format verbose"
  },
  "dependencies": {
    "@microsoft/fluid-component-core-interfaces": "^0.12.0",
    "@microsoft/fluid-container-definitions": "^0.12.0",
    "@microsoft/fluid-core-utils": "^0.12.0",
    "@microsoft/fluid-driver-definitions": "^0.12.0",
    "@microsoft/fluid-driver-utils": "^0.12.0",
    "@microsoft/fluid-protocol-definitions": "^0.1000.0",
    "comlink": "^4.0.2",
    "debug": "^4.1.1"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.4.4",
    "@microsoft/eslint-config-fluid": "^0.12.0",
    "@microsoft/fluid-build-common": "^0.12.0",
    "@types/debug": "^0.0.31",
    "@types/mocha": "^5.2.5",
    "@types/nock": "^9.3.0",
    "@types/socket.io-client": "^1.4.32",
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "concurrently": "^4.1.0",
    "eslint": "^6.7.1",
    "mocha": "^5.2.0",
    "nock": "^10.0.1",
    "rimraf": "^2.6.2",
    "tslint": "^5.20.0",
    "typescript": "~3.4.5"
  }
}
