{
  "name": "@fluidframework/routerlicious-host",
  "version": "0.36.0",
  "description": "Routerlicious host interfaces",
  "homepage": "https://fluidframework.com",
  "repository": "https://github.com/microsoft/FluidFramework",
  "license": "MIT",
  "author": "Microsoft",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "pnpm run build:genver && concurrently pnpm:build:compile pnpm:lint",
    "build:commonjs": "pnpm run tsc && pnpm run build:test",
    "build:compile": "concurrently pnpm:build:commonjs pnpm:build:esnext",
    "build:esnext": "tsc --project ./tsconfig.esnext.json",
    "build:full": "pnpm run build",
    "build:full:compile": "pnpm run build:compile",
    "build:genver": "gen-version",
    "build:test": "tsc --project ./src/test/tsconfig.json",
    "clean": "rimraf dist lib *.tsbuildinfo *.build.log",
    "eslint": "eslint --format stylish src",
    "eslint:fix": "eslint --format stylish src --fix",
    "lint": "pnpm run eslint",
    "lint:fix": "pnpm run eslint:fix",
    "tsc": "tsc",
    "tsfmt": "tsfmt --verify",
    "tsfmt:fix": "tsfmt --replace"
  },
  "dependencies": {
    "@fluidframework/common-utils": "^0.27.0",
    "@fluidframework/core-interfaces": "^0.36.0",
    "@fluidframework/driver-definitions": "^0.36.0",
    "@types/debug": "^4.1.5",
    "axios": "^0.21.1",
    "debug": "^4.1.1"
  },
  "devDependencies": {
    "@fluidframework/build-common": "^0.20.0-0",
    "@fluidframework/eslint-config-fluid": "^0.23.0-0",
    "@types/assert": "^1.5.2",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.17.24",
    "@typescript-eslint/eslint-plugin": "~4.14.0",
    "@typescript-eslint/parser": "~4.14.0",
    "assert": "^2.0.0",
    "concurrently": "^5.2.0",
    "eslint": "~7.18.0",
    "eslint-plugin-eslint-comments": "~3.2.0",
    "eslint-plugin-import": "~2.22.1",
    "eslint-plugin-no-null": "~1.0.2",
    "eslint-plugin-prefer-arrow": "~1.2.2",
    "eslint-plugin-react": "~7.22.0",
    "eslint-plugin-unicorn": "~26.0.1",
    "mocha": "^8.1.1",
    "rimraf": "^2.6.2",
    "typescript": "~4.1.3",
    "typescript-formatter": "7.1.0"
  }
}
