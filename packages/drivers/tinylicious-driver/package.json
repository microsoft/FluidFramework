{
  "name": "@fluidframework/tinylicious-driver",
  "version": "0.32.0",
  "description": "Driver for tinylicious",
  "homepage": "https://fluidframework.com",
  "repository": "https://github.com/microsoft/FluidFramework",
  "license": "MIT",
  "author": "Microsoft",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "concurrently \"yarn run build:compile\" \"yarn run lint\"",
    "build:commonjs": "yarn run tsc && yarn run build:test",
    "build:compile": "concurrently \"yarn run build:commonjs\" \"yarn run build:esnext\"",
    "build:esnext": "tsc --project ./tsconfig.esnext.json",
    "build:full": "yarn run build",
    "build:full:compile": "yarn run build:compile",
    "build:test": "tsc --project ./src/test/tsconfig.json",
    "clean": "rimraf dist lib *.tsbuildinfo *.build.log",
    "eslint": "eslint --format stylish src",
    "eslint:fix": "eslint --format stylish src --fix",
    "lint": "yarn run eslint",
    "lint:fix": "yarn run eslint:fix",
    "tsc": "tsc",
    "tsfmt": "tsfmt --verify",
    "tsfmt:fix": "tsfmt --replace"
  },
  "dependencies": {
    "@fluidframework/core-interfaces": "^0.32.0",
    "@fluidframework/driver-definitions": "^0.32.0",
    "@fluidframework/driver-utils": "^0.32.0",
    "@fluidframework/protocol-definitions": "^0.1017.0-0",
    "@fluidframework/routerlicious-driver": "^0.32.0",
    "jsrsasign": "^10.0.2",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@fluidframework/build-common": "^0.20.0",
    "@fluidframework/eslint-config-fluid": "^0.22.0",
    "@fluidframework/test-tools": "^0.2.3074",
    "@types/jsrsasign": "^8.0.8",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.17.24",
    "@typescript-eslint/eslint-plugin": "~4.2.0",
    "@typescript-eslint/parser": "~4.2.0",
    "concurrently": "^5.2.0",
    "eslint": "~7.11.0",
    "mocha": "^8.1.1",
    "rimraf": "^2.6.2",
    "typescript": "~3.7.4",
    "typescript-formatter": "7.1.0"
  }
}
