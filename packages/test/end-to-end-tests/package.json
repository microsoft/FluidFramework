{
  "name": "@fluid-internal/end-to-end-tests",
  "version": "0.30.0",
  "private": true,
  "description": "End to end tests",
  "homepage": "https://fluidframework.com",
  "repository": "https://github.com/microsoft/FluidFramework",
  "license": "MIT",
  "author": "Microsoft",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build:genver && concurrently npm:build:compile npm:lint",
    "build:compile": "concurrently npm:tsc npm:build:esnext",
    "build:esnext": "tsc --project ./tsconfig.esnext.json",
    "build:full": "npm run build",
    "build:full:compile": "npm run build:compile",
    "build:genver": "gen-version",
    "ci:test:realsvc": "npm run test:realsvc:mocha:report; exit 0",
    "clean": "rimraf dist lib *.tsbuildinfo *.build.log",
    "eslint": "eslint --format stylish src",
    "eslint:fix": "eslint --ext=ts,tsx --format stylish src --fix",
    "lint": "npm run eslint",
    "lint:fix": "npm run eslint:fix",
    "test": "npm run test:mocha",
    "test:coverage": "nyc npm test -- --reporter mocha-junit-reporter --reporter-options mochaFile=nyc/junit-report.xml",
    "test:mocha": "mocha --recursive dist/test --exit -r node_modules/@fluidframework/mocha-test-setup --unhandled-rejections=strict",
    "test:mocha:verbose": "cross-env FLUID_TEST_VERBOSE=1 npm run test:mocha",
    "test:realsvc:mocha": "mocha --recursive dist/real-service-tests --exit -r node_modules/@fluidframework/mocha-test-setup --unhandled-rejections=strict",
    "test:realsvc:mocha:report": "npm run test:realsvc:mocha -- --timeout 4s --reporter mocha-junit-reporter --reporter-options mochaFile=nyc/mocha-realsvc-junit-report.xml",
    "tsc": "tsc",
    "tsfmt": "tsfmt --verify",
    "tsfmt:fix": "tsfmt --replace"
  },
  "nyc": {
    "all": true,
    "cache-dir": "nyc/.cache",
    "exclude": [
      "src/test/**/*.ts",
      "dist/test/**/*.js"
    ],
    "exclude-after-remap": false,
    "include": [
      "src/**/*.ts",
      "dist/**/*.js"
    ],
    "report-dir": "nyc/report",
    "reporter": [
      "cobertura",
      "html",
      "text"
    ],
    "temp-directory": "nyc/.nyc_output"
  },
  "devDependencies": {
    "@fluid-internal/test-loader-utils": "^0.30.0",
