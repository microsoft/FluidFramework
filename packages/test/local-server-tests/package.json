{
  "name": "@fluid-internal/local-server-tests",
  "version": "0.34.0",
  "private": true,
  "description": "Tests that can only run against the local server",
  "homepage": "https://fluidframework.com",
  "repository": "https://github.com/microsoft/FluidFramework",
  "license": "MIT",
  "author": "Microsoft",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build:genver && concurrently npm:build:compile npm:lint",
    "build:compile": "concurrently npm:tsc npm:build:esnext",
    "build:esnext": "tsc --project ./tsconfig.esnext.json",
    "build:full": "npm run build",
    "build:full:compile": "npm run build:compile",
    "build:genver": "gen-version",
    "ci:test:realsvc": "npm run test:realsvc:mocha:report; exit 0",
    "clean": "rimraf dist lib *.tsbuildinfo *.build.log",
    "eslint": "eslint --format stylish src",
    "eslint:fix": "eslint --format stylish src --fix",
    "lint": "npm run eslint",
    "lint:fix": "npm run eslint:fix",
    "test": "npm run test:mocha",
    "test:mocha:verbose": "cross-env FLUID_TEST_VERBOSE=1 npm run test:mocha",
    "test:coverage": "cross-env FLUID_TEST_COVERAGE=1 npm run test:mocha",
    "test:mocha": "mocha dist/test",
    "tsc": "tsc",
    "tsfmt": "tsfmt --verify",
    "tsfmt:fix": "tsfmt --replace"
  },
  "nyc": {
    "all": true,
    "cache-dir": "nyc/.cache",
    "exclude": [
      "src/test/**/*.ts",
      "dist/test/**/*.js"
    ],
    "exclude-after-remap": false,
    "include": [
      "src/**/*.ts",
      "dist/**/*.js"
    ],
    "report-dir": "nyc/report",
    "reporter": [
      "cobertura",
      "html",
      "text"
    ],
    "temp-directory": "nyc/.nyc_output"
  },
  "devDependencies": {
    "@fluid-internal/test-loader-utils": "^0.34.0",
    "@fluidframework/agent-scheduler": "^0.34.0",
    "@fluidframework/aqueduct": "^0.34.0",
    "@fluidframework/base-host": "^0.34.0",
    "@fluidframework/build-common": "^0.20.0-0",
    "@fluidframework/cell": "^0.34.0",
    "@fluidframework/common-utils": "^0.27.0",
    "@fluidframework/container-definitions": "^0.34.0",
    "@fluidframework/container-loader": "^0.34.0",
    "@fluidframework/container-runtime": "^0.34.0",
    "@fluidframework/container-runtime-definitions": "^0.34.0",
    "@fluidframework/container-utils": "^0.34.0",
    "@fluidframework/core-interfaces": "^0.34.0",
    "@fluidframework/counter": "^0.34.0",
    "@fluidframework/datastore": "^0.34.0",
    "@fluidframework/datastore-definitions": "^0.34.0",
    "@fluidframework/driver-base": "^0.34.0",
    "@fluidframework/driver-definitions": "^0.34.0",
    "@fluidframework/driver-utils": "^0.34.0",
    "@fluidframework/eslint-config-fluid": "^0.22.1-0",
    "@fluidframework/ink": "^0.34.0",
    "@fluidframework/local-driver": "^0.34.0",
    "@fluidframework/map": "^0.34.0",
    "@fluidframework/matrix": "^0.34.0",
    "@fluidframework/merge-tree": "^0.34.0",
    "@fluidframework/mocha-test-setup": "^0.34.0",
    "@fluidframework/odsp-doclib-utils": "^0.34.0",
    "@fluidframework/ordered-collection": "^0.34.0",
    "@fluidframework/protocol-definitions": "^0.1018.0",
    "@fluidframework/register-collection": "^0.34.0",
    "@fluidframework/request-handler": "^0.34.0",
    "@fluidframework/routerlicious-driver": "^0.34.0",
    "@fluidframework/runtime-definitions": "^0.34.0",
    "@fluidframework/runtime-utils": "^0.34.0",
    "@fluidframework/sequence": "^0.34.0",
    "@fluidframework/server-local-server": "^0.1018.0",
    "@fluidframework/shared-object-base": "^0.34.0",
    "@fluidframework/telemetry-utils": "^0.34.0",
    "@fluidframework/test-drivers": "^0.34.0",
    "@fluidframework/test-runtime-utils": "^0.34.0",
    "@fluidframework/test-utils": "^0.34.0",
    "@types/assert": "^1.5.1",
    "@types/mocha": "^5.2.5",
    "@types/nock": "^9.3.0",
    "@types/node": "^10.17.24",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "~4.14.0",
    "@typescript-eslint/parser": "~4.14.0",
    "assert": "^2.0.0",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "eslint": "~7.18.0",
    "eslint-plugin-eslint-comments": "~3.2.0",
    "eslint-plugin-import": "~2.22.1",
    "eslint-plugin-no-null": "~1.0.2",
    "eslint-plugin-prefer-arrow": "~1.2.2",
    "eslint-plugin-react": "~7.22.0",
    "eslint-plugin-unicorn": "~26.0.1",
    "mocha": "^8.1.1",
    "mocha-junit-reporter": "^1.18.0",
    "moniker": "^0.1.2",
    "nock": "^10.0.1",
    "nyc": "^15.0.0",
    "rimraf": "^2.6.2",
    "start-server-and-test": "^1.11.7",
    "tinylicious": "^0.4.13835",
    "ts-loader": "^6.1.2",
    "typescript": "~3.7.4",
    "typescript-formatter": "7.1.0",
    "uuid": "^8.3.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
