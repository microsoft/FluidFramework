{
  "name": "@fluid-internal/local-server-tests",
  "version": "0.39.4",
  "private": true,
  "description": "Tests that can only run against the local server",
  "homepage": "https://fluidframework.com",
  "repository": "https://github.com/microsoft/FluidFramework",
  "license": "MIT",
  "author": "Microsoft and contributors",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "concurrently npm:build:compile npm:lint",
    "build:compile": "npm run build:test",
    "build:full": "npm run build",
    "build:full:compile": "npm run build:compile",
    "build:test": "tsc --project ./src/test/tsconfig.json",
    "clean": "rimraf dist lib *.tsbuildinfo *.build.log",
    "eslint": "eslint --format stylish src",
    "eslint:fix": "eslint --format stylish src --fix",
    "lint": "npm run eslint",
    "lint:fix": "npm run eslint:fix",
    "test": "npm run test:mocha",
    "test:coverage": "cross-env FLUID_TEST_COVERAGE=1 npm run test:mocha",
    "test:mocha": "mocha dist/test -r node_modules/@fluidframework/mocha-test-setup --unhandled-rejections=strict --exit",
    "test:mocha:verbose": "cross-env FLUID_TEST_VERBOSE=1 npm run test:mocha",
    "tsfmt": "tsfmt --verify",
    "tsfmt:fix": "tsfmt --replace"
  },
  "nyc": {
    "all": true,
    "cache-dir": "nyc/.cache",
    "exclude": [
      "src/test/**/*.ts",
      "dist/test/**/*.js"
    ],
    "exclude-after-remap": false,
    "include": [
      "src/**/*.ts",
      "dist/**/*.js"
    ],
    "report-dir": "nyc/report",
    "reporter": [
      "cobertura",
      "html",
      "text"
    ],
    "temp-directory": "nyc/.nyc_output"
  },
  "devDependencies": {
    "@fluidframework/agent-scheduler": "^0.39.4",
    "@fluidframework/aqueduct": "^0.39.4",
    "@fluidframework/base-host": "^0.39.4",
    "@fluidframework/build-common": "^0.22.0",
    "@fluidframework/cell": "^0.39.4",
    "@fluidframework/common-utils": "^0.30.0",
    "@fluidframework/container-definitions": "^0.39.4",
    "@fluidframework/container-loader": "^0.39.4",
    "@fluidframework/container-runtime": "^0.39.4",
    "@fluidframework/container-runtime-definitions": "^0.39.4",
    "@fluidframework/container-utils": "^0.39.4",
    "@fluidframework/core-interfaces": "^0.39.4",
    "@fluidframework/counter": "^0.39.4",
    "@fluidframework/datastore": "^0.39.4",
    "@fluidframework/datastore-definitions": "^0.39.4",
    "@fluidframework/driver-base": "^0.39.4",
    "@fluidframework/driver-definitions": "^0.39.4",
    "@fluidframework/driver-utils": "^0.39.4",
    "@fluidframework/eslint-config-fluid": "^0.23.0",
    "@fluidframework/ink": "^0.39.4",
    "@fluidframework/local-driver": "^0.39.4",
    "@fluidframework/map": "^0.39.4",
    "@fluidframework/matrix": "^0.39.4",
    "@fluidframework/merge-tree": "^0.39.4",
    "@fluidframework/mocha-test-setup": "^0.39.4",
    "@fluidframework/odsp-doclib-utils": "^0.39.4",
    "@fluidframework/ordered-collection": "^0.39.4",
    "@fluidframework/protocol-definitions": "^0.1024.0",
    "@fluidframework/register-collection": "^0.39.4",
    "@fluidframework/request-handler": "^0.39.4",
    "@fluidframework/routerlicious-driver": "^0.39.4",
    "@fluidframework/runtime-definitions": "^0.39.4",
    "@fluidframework/runtime-utils": "^0.39.4",
    "@fluidframework/sequence": "^0.39.4",
    "@fluidframework/server-local-server": "^0.1024.0",
    "@fluidframework/shared-object-base": "^0.39.4",
    "@fluidframework/telemetry-utils": "^0.39.4",
    "@fluidframework/test-loader-utils": "^0.39.4",
    "@fluidframework/test-runtime-utils": "^0.39.4",
    "@fluidframework/test-utils": "^0.39.4",
    "@types/assert": "^1.5.2",
    "@types/mocha": "^5.2.5",
    "@types/nock": "^9.3.0",
    "@types/node": "^12.19.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "~4.14.0",
    "@typescript-eslint/parser": "~4.14.0",
    "assert": "^2.0.0",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "eslint": "~7.18.0",
    "eslint-plugin-eslint-comments": "~3.2.0",
    "eslint-plugin-import": "~2.22.1",
    "eslint-plugin-no-null": "~1.0.2",
    "eslint-plugin-prefer-arrow": "~1.2.2",
    "eslint-plugin-react": "~7.22.0",
    "eslint-plugin-unicorn": "~26.0.1",
    "mocha": "^8.1.1",
    "moniker": "^0.1.2",
    "nock": "^10.0.1",
    "nyc": "^15.0.0",
    "rimraf": "^2.6.2",
    "start-server-and-test": "^1.11.7",
    "tinylicious": "^0.4.21640",
    "ts-loader": "^6.1.2",
    "typescript": "~4.1.3",
    "typescript-formatter": "7.1.0",
    "uuid": "^8.3.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
