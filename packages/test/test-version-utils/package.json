{
  "name": "@fluidframework/test-version-utils",
  "version": "2.0.0-internal.1.3.0",
  "description": "End to end tests",
  "homepage": "https://fluidframework.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/FluidFramework.git",
    "directory": "packages/test/test-version-utils"
  },
  "license": "MIT",
  "author": "Microsoft and contributors",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build:genver && concurrently npm:build:compile npm:lint",
    "build:compile": "npm run tsc && npm run build:test",
    "build:full": "npm run build",
    "build:full:compile": "npm run build:compile",
    "build:genver": "gen-version",
    "build:test": "tsc --project ./src/test/tsconfig.json",
    "clean": "rimraf dist lib *.tsbuildinfo *.build.log node_modules/.legacy",
    "eslint": "eslint --format stylish src",
    "eslint:fix": "eslint --format stylish src --fix --fix-type problem,suggestion,layout",
    "lint": "npm run eslint",
    "lint:fix": "npm run eslint:fix",
    "test": "npm run test:mocha",
    "test:mocha": "mocha --unhandled-rejections=strict  --recursive dist/test/*.spec.js --exit --project src/test/tsconfig.json -r node_modules/@fluidframework/mocha-test-setup",
    "test:mocha:verbose": "cross-env FLUID_TEST_VERBOSE=1 npm run test:mocha",
    "tsc": "tsc",
    "tsfmt": "tsfmt --verify",
    "tsfmt:fix": "tsfmt --replace"
  },
  "nyc": {
    "all": true,
    "cache-dir": "nyc/.cache",
    "exclude": [
      "src/test/**/*.ts",
      "dist/test/**/*.js"
    ],
    "exclude-after-remap": false,
    "include": [
      "src/**/*.ts",
      "dist/**/*.js"
    ],
    "report-dir": "nyc/report",
    "reporter": [
      "cobertura",
      "html",
      "text"
    ],
    "temp-directory": "nyc/.nyc_output"
  },
  "dependencies": {
    "@fluidframework/aqueduct": ">=2.0.0-internal.1.3.0 <2.0.0-internal.2.0.0",
    "@fluidframework/cell": ">=2.0.0-internal.1.3.0 <2.0.0-internal.2.0.0",
    "@fluidframework/common-definitions": "^0.20.1",
    "@fluidframework/common-utils": "^1.0.0",
    "@fluidframework/container-definitions": ">=2.0.0-internal.1.3.0 <2.0.0-internal.2.0.0",
    "@fluidframework/container-loader": ">=2.0.0-internal.1.3.0 <2.0.0-internal.2.0.0",
    "@fluidframework/container-runtime": ">=2.0.0-internal.1.3.0 <2.0.0-internal.2.0.0",
    "@fluidframework/core-interfaces": ">=2.0.0-internal.1.3.0 <2.0.0-internal.2.0.0",
    "@fluidframework/counter": ">=2.0.0-internal.1.3.0 <2.0.0-internal.2.0.0",
    "@fluidframework/datastore-definitions": ">=2.0.0-internal.1.3.0 <2.0.0-internal.2.0.0",
    "@fluidframework/driver-definitions": ">=2.0.0-internal.1.3.0 <2.0.0-internal.2.0.0",
    "@fluidframework/driver-utils": ">=2.0.0-internal.1.3.0 <2.0.0-internal.2.0.0",
    "@fluidframework/ink": ">=2.0.0-internal.1.3.0 <2.0.0-internal.2.0.0",
    "@fluidframework/map": ">=2.0.0-internal.1.3.0 <2.0.0-internal.2.0.0",
    "@fluidframework/matrix": ">=2.0.0-internal.1.3.0 <2.0.0-internal.2.0.0",
    "@fluidframework/ordered-collection": ">=2.0.0-internal.1.3.0 <2.0.0-internal.2.0.0",
    "@fluidframework/protocol-definitions": "^1.0.0",
    "@fluidframework/register-collection": ">=2.0.0-internal.1.3.0 <2.0.0-internal.2.0.0",
    "@fluidframework/runtime-definitions": ">=2.0.0-internal.1.3.0 <2.0.0-internal.2.0.0",
    "@fluidframework/sequence": ">=2.0.0-internal.1.3.0 <2.0.0-internal.2.0.0",
    "@fluidframework/test-driver-definitions": ">=2.0.0-internal.1.3.0 <2.0.0-internal.2.0.0",
    "@fluidframework/test-drivers": ">=2.0.0-internal.1.3.0 <2.0.0-internal.2.0.0",
    "@fluidframework/test-utils": ">=2.0.0-internal.1.3.0 <2.0.0-internal.2.0.0",
    "nconf": "^0.11.4",
    "proper-lockfile": "^4.1.2",
    "semver": "^7.3.4"
  },
  "devDependencies": {
    "@fluidframework/build-common": "^1.0.0",
    "@fluidframework/build-tools": "^0.4.6000",
    "@fluidframework/eslint-config-fluid": "^1.0.0",
    "@fluidframework/mocha-test-setup": ">=2.0.0-internal.1.3.0 <2.0.0-internal.2.0.0",
    "@fluidframework/test-version-utils-previous": "npm:@fluidframework/test-version-utils@^1.0.0",
    "@rushstack/eslint-config": "^2.5.1",
    "@types/mocha": "^9.1.1",
    "@types/nock": "^9.3.0",
    "@types/node": "^14.18.0",
    "@types/uuid": "^8.3.0",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.2",
    "eslint": "~8.6.0",
    "mocha": "^10.0.0",
    "nock": "^10.0.1",
    "nyc": "^15.0.0",
    "rimraf": "^2.6.2",
    "ts-loader": "^9.3.0",
    "typescript": "~4.5.5",
    "typescript-formatter": "7.1.0",
    "uuid": "^8.3.1",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  },
  "typeValidation": {
    "version": "2.0.0",
    "broken": {
      "InterfaceDeclaration_ITestDataObject": {
        "forwardCompat": false
      }
    }
  }
}
