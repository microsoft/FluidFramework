{
  "name": "@fluid-internal/end-to-end-tests",
  "version": "0.19.0",
  "description": "End to end tests",
  "repository": "microsoft/FluidFramework",
  "license": "MIT",
  "author": "Microsoft",
  "sideEffects": "false",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build:genver && concurrently npm:build:compile npm:lint",
    "build:compile": "concurrently npm:tsc npm:build:esnext",
    "build:esnext": "tsc --project ./tsconfig.esnext.json",
    "build:full": "npm run build",
    "build:full:compile": "npm run build:compile",
    "build:genver": "gen-version",
    "clean": "rimraf dist lib *.tsbuildinfo *.build.log",
    "eslint": "eslint --ext=ts,tsx --format stylish src",
    "eslint:fix": "eslint --ext=ts,tsx --format stylish src --fix",
    "lint": "npm run eslint",
    "lint:fix": "npm run eslint:fix",
    "test": "npm run test:mocha",
    "test:coverage": "nyc npm test -- --reporter mocha-junit-reporter --reporter-options mochaFile=nyc/junit-report.xml",
    "test:mocha": "mocha --recursive dist/test --exit -r make-promises-safe",
    "tsc": "tsc"
  },
  "nyc": {
    "all": true,
    "cache-dir": "nyc/.cache",
    "exclude": [
      "src/test/**/*.ts",
      "dist/test/**/*.js"
    ],
    "exclude-after-remap": false,
    "include": [
      "src/**/*.ts",
      "dist/**/*.js"
    ],
    "report-dir": "nyc/report",
    "reporter": [
      "cobertura",
      "html",
      "text"
    ],
    "temp-directory": "nyc/.nyc_output"
  },
  "devDependencies": {
    "@fluidframework/agent-scheduler": "^0.19.0",
    "@fluidframework/aqueduct": "^0.19.0",
    "@fluidframework/base-host": "^0.19.0",
    "@microsoft/fluid-build-common": "^0.16.0-0",
    "@fluidframework/cell": "^0.19.0",
    "@fluidframework/component-core-interfaces": "^0.19.0",
    "@fluidframework/component-runtime": "^0.19.0",
    "@fluidframework/component-runtime-definitions": "^0.19.0",
    "@fluidframework/container-definitions": "^0.19.0",
    "@fluidframework/container-loader": "^0.19.0",
    "@fluidframework/container-runtime-definitions": "^0.19.0",
    "@fluidframework/driver-base": "^0.19.0",
    "@fluidframework/driver-definitions": "^0.19.0",
    "@fluidframework/driver-utils": "^0.19.0",
    "@fluidframework/eslint-config-fluid": "^0.17.0-0",
    "@fluidframework/experimental-creation-driver": "^0.19.0",
    "@fluidframework/local-driver": "^0.19.0",
    "@fluidframework/local-test-utils": "^0.19.0",
    "@fluidframework/map": "^0.19.0",
    "@fluidframework/merge-tree": "^0.19.0",
    "@fluidframework/odsp-driver": "^0.19.0",
    "@fluidframework/ordered-collection": "^0.19.0",
    "@fluidframework/protocol-definitions": "^0.1006.0-0",
    "@fluidframework/register-collection": "^0.19.0",
    "@fluidframework/runtime-definitions": "^0.19.0",
    "@fluidframework/sequence": "^0.19.0",
    "@fluidframework/server-local-server": "^0.1006.0-0",
    "@fluidframework/shared-object-base": "^0.19.0",
    "@fluidframework/test-loader-utils": "^0.19.0",
    "@fluidframework/test-utils": "^0.19.0",
    "@types/mocha": "^5.2.5",
    "@types/nock": "^9.3.0",
    "@types/node": "^10.14.6",
    "@types/uuid": "^3.4.4",
    "@typescript-eslint/eslint-plugin": "~2.17.0",
    "@typescript-eslint/parser": "~2.17.0",
    "concurrently": "^4.1.0",
    "eslint": "~6.8.0",
    "eslint-plugin-eslint-comments": "~3.1.2",
    "eslint-plugin-import": "2.20.0",
    "eslint-plugin-no-null": "~1.0.2",
    "eslint-plugin-optimize-regex": "~1.1.7",
    "eslint-plugin-prefer-arrow": "~1.1.7",
    "eslint-plugin-react": "~7.18.0",
    "eslint-plugin-unicorn": "~15.0.1",
    "make-promises-safe": "^5.1.0",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.18.0",
    "nock": "^10.0.1",
    "nyc": "^15.0.0",
    "old-aqueduct": "npm:@microsoft/fluid-aqueduct@^0.18.1",
    "old-container-definitions": "npm:@microsoft/fluid-container-definitions@^0.18.1",
    "old-container-loader": "npm:@microsoft/fluid-container-loader@^0.18.1",
    "old-runtime-definitions": "npm:@microsoft/fluid-runtime-definitions@^0.18.1",
    "old-test-utils": "npm:@microsoft/fluid-test-utils@^0.18.1",
    "rimraf": "^2.6.2",
    "ts-loader": "^6.1.2",
    "typescript": "~3.7.4",
    "uuid": "^3.3.2",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.2"
  }
}
