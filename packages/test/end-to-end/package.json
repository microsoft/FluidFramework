{
  "name": "@fluid-internal/end-to-end-tests",
  "version": "0.12.0",
  "description": "End to end tests",
  "repository": "microsoft/FluidFramework",
  "license": "MIT",
  "author": "Microsoft",
  "sideEffects": "false",
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build:genver && concurrently npm:build:compile npm:tslint",
    "build:compile": "concurrently npm:tsc npm:build:esnext",
    "build:esnext": "tsc --project ./tsconfig.esnext.json",
    "build:full": "npm run build",
    "build:full:compile": "npm run build:compile",
    "build:genver": "node node_modules/@microsoft/fluid-build-common/gen_version.js",
    "clean": "rimraf dist lib *.tsbuildinfo package-deps.json *.build.log",
    "test": "mocha --recursive dist/test --exit",
    "test:coverage": "nyc npm test -- --reporter mocha-junit-reporter --reporter-options mochaFile=nyc/junit-report.xml",
    "tsc": "tsc",
    "tslint": "tslint --project tsconfig.json --format verbose"
  },
  "devDependencies": {
    "@fluid-internal/client-api": "^0.12.0",
    "@microsoft/fluid-agent-scheduler": "^0.12.0",
    "@microsoft/fluid-aqueduct": "^0.12.0",
    "@microsoft/fluid-build-common": "^0.12.0",
    "@microsoft/fluid-cell": "^0.12.0",
    "@microsoft/fluid-component-core-interfaces": "^0.12.0",
    "@microsoft/fluid-container-definitions": "^0.12.0",
    "@microsoft/fluid-container-loader": "^0.12.0",
    "@microsoft/fluid-local-test-server": "^0.12.0",
    "@microsoft/fluid-map": "^0.12.0",
    "@microsoft/fluid-merge-tree": "^0.12.0",
    "@microsoft/fluid-ordered-collection": "^0.12.0",
    "@microsoft/fluid-protocol-definitions": "^0.12.0",
    "@microsoft/fluid-register-collection": "^0.12.0",
    "@microsoft/fluid-runtime-definitions": "^0.12.0",
    "@microsoft/fluid-sequence": "^0.12.0",
    "@types/mocha": "^5.2.5",
    "@types/nock": "^9.3.0",
    "@types/node": "^10.12.12",
    "concurrently": "^4.1.0",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.18.0",
    "nock": "^10.0.1",
    "nyc": "^14.1.1",
    "rimraf": "^2.6.2",
    "ts-loader": "^4.5.0",
    "tslint": "^5.11.0",
    "typescript": "~3.4.5",
    "webpack": "^4.8.2",
    "webpack-cli": "^3.1.2"
  },
  "nyc": {
    "all": true,
    "exclude": [
      "src/test/**/*.ts",
      "dist/test/**/*.js"
    ],
    "include": [
      "src/**/*.ts",
      "dist/**/*.js"
    ],
    "reporter": [
      "cobertura",
      "html",
      "text"
    ],
    "temp-directory": "nyc/.nyc_output",
    "cache-dir": "nyc/.cache",
    "report-dir": "nyc/report"
  }
}
