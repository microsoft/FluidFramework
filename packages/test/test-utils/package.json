{
  "name": "@fluidframework/test-utils",
  "version": "2.0.0-internal.1.4.2",
  "description": "Utilities for Fluid tests",
  "homepage": "https://fluidframework.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/microsoft/FluidFramework.git",
    "directory": "packages/test/test-utils"
  },
  "license": "MIT",
  "author": "Microsoft and contributors",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "lib/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "npm run build:genver && concurrently npm:build:compile npm:lint && npm run build:docs",
    "build:compile": "npm run tsc && npm run typetests:gen && npm run build:test",
    "build:compile:min": "npm run build:compile",
    "build:docs": "api-extractor run --local --typescript-compiler-folder ../../../node_modules/typescript && copyfiles -u 1 ./_api-extractor-temp/doc-models/* ../../../_api-extractor-temp/",
    "build:full": "npm run build",
    "build:full:compile": "npm run build:compile",
    "build:genver": "gen-version",
    "build:test": "tsc --project ./src/test/tsconfig.json",
    "ci:build:docs": "api-extractor run --typescript-compiler-folder ../../../node_modules/typescript && copyfiles -u 1 ./_api-extractor-temp/* ../../../_api-extractor-temp/",
    "clean": "rimraf dist lib *.tsbuildinfo *.build.log",
    "eslint": "eslint --format stylish src",
    "eslint:fix": "eslint --format stylish src --fix --fix-type problem,suggestion,layout",
    "lint": "npm run eslint",
    "lint:fix": "npm run eslint:fix",
    "tsc": "tsc",
    "tsfmt": "tsfmt --verify",
    "tsfmt:fix": "tsfmt --replace",
    "typetests:gen": "fluid-type-validator -g -d ."
  },
  "nyc": {
    "all": true,
    "cache-dir": "nyc/.cache",
    "exclude": [
      "src/test/**/*.ts",
      "dist/test/**/*.js"
    ],
    "exclude-after-remap": false,
    "include": [
      "src/**/*.ts",
      "dist/**/*.js"
    ],
    "report-dir": "nyc/report",
    "reporter": [
      "cobertura",
      "html",
      "text"
    ],
    "temp-directory": "nyc/.nyc_output"
  },
  "dependencies": {
    "@fluidframework/aqueduct": ">=2.0.0-internal.1.4.2 <2.0.0-internal.2.0.0",
    "@fluidframework/common-definitions": "^0.20.1",
    "@fluidframework/common-utils": "^1.0.0",
    "@fluidframework/container-definitions": ">=2.0.0-internal.1.4.2 <2.0.0-internal.2.0.0",
    "@fluidframework/container-loader": ">=2.0.0-internal.1.4.2 <2.0.0-internal.2.0.0",
    "@fluidframework/container-runtime": ">=2.0.0-internal.1.4.2 <2.0.0-internal.2.0.0",
    "@fluidframework/container-runtime-definitions": ">=2.0.0-internal.1.4.2 <2.0.0-internal.2.0.0",
    "@fluidframework/core-interfaces": ">=2.0.0-internal.1.4.2 <2.0.0-internal.2.0.0",
    "@fluidframework/datastore": ">=2.0.0-internal.1.4.2 <2.0.0-internal.2.0.0",
    "@fluidframework/datastore-definitions": ">=2.0.0-internal.1.4.2 <2.0.0-internal.2.0.0",
    "@fluidframework/driver-definitions": ">=2.0.0-internal.1.4.2 <2.0.0-internal.2.0.0",
    "@fluidframework/driver-utils": ">=2.0.0-internal.1.4.2 <2.0.0-internal.2.0.0",
    "@fluidframework/local-driver": ">=2.0.0-internal.1.4.2 <2.0.0-internal.2.0.0",
    "@fluidframework/map": ">=2.0.0-internal.1.4.2 <2.0.0-internal.2.0.0",
    "@fluidframework/protocol-definitions": "^1.0.0",
    "@fluidframework/request-handler": ">=2.0.0-internal.1.4.2 <2.0.0-internal.2.0.0",
    "@fluidframework/routerlicious-driver": ">=2.0.0-internal.1.4.2 <2.0.0-internal.2.0.0",
    "@fluidframework/runtime-definitions": ">=2.0.0-internal.1.4.2 <2.0.0-internal.2.0.0",
    "@fluidframework/runtime-utils": ">=2.0.0-internal.1.4.2 <2.0.0-internal.2.0.0",
    "@fluidframework/telemetry-utils": ">=2.0.0-internal.1.4.2 <2.0.0-internal.2.0.0",
    "@fluidframework/test-driver-definitions": ">=2.0.0-internal.1.4.2 <2.0.0-internal.2.0.0",
    "@fluidframework/test-runtime-utils": ">=2.0.0-internal.1.4.2 <2.0.0-internal.2.0.0",
    "best-random": "^1.0.0",
    "debug": "^4.1.1",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@fluidframework/build-common": "^1.0.0",
    "@fluidframework/build-tools": "^0.4.4000",
    "@fluidframework/eslint-config-fluid": "^1.0.0",
    "@fluidframework/test-utils-previous": "npm:@fluidframework/test-utils@^1.0.0",
    "@microsoft/api-extractor": "^7.22.2",
    "@rushstack/eslint-config": "^2.5.1",
    "@types/diff": "^3.5.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^14.18.0",
    "@types/random-js": "^1.0.31",
    "concurrently": "^6.2.0",
    "copyfiles": "^2.4.1",
    "diff": "^3.5.0",
    "eslint": "~8.6.0",
    "mocha": "^10.0.0",
    "nyc": "^15.0.0",
    "random-js": "^1.0.8",
    "rimraf": "^2.6.2",
    "typescript": "~4.5.5",
    "typescript-formatter": "7.1.0"
  },
  "typeValidation": {
    "version": "2.0.0",
    "broken": {}
  }
}
