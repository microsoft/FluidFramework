{
	// This node report is always meant to be identical to the browser report. Do not use with --local.
	"$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
	"extends": "../../../../common/build/build-common/api-extractor-report.esm.current.json",
	"mainEntryPointFilePath": "<projectFolder>/lib/<unscopedPackageName>.node.alpha.d.ts",
	"apiReport": {
		// This is an internal package and should have no public API surface,
		// but it does have a legacy API. Since there should be none, report all
		// variants and expect no content (apart from the EventEmitter forward).
		"enabled": true,
		"reportVariants": ["public", "beta", "alpha"]
	}
}
