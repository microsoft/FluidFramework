{
	"root": false,
	"overrides": [
		{
			"includes": [
				"**/.vscode/**/*.json",
				"**/tsconfig*.json",
				"**/api-extractor*.json",
				"**/tsdoc*.json"
			],
			"json": {
				"parser": {
					"allowComments": true,
					"allowTrailingCommas": true
				},
				"formatter": {
					"trailingCommas": "all"
				}
			}
		}
	]
}
