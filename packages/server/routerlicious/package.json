{
  "name": "@prague/routerlicious",
  "version": "0.9.0",
  "description": "Server to assign sequence numbers and route deltas between connected clients",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist public/scripts/dist *.tsbuildinfo package-deps.json *.build.log",
    "build": "concurrently npm:build:compile npm:tslint",
    "build:compile": "npm run tsc",
    "build:full": "npm run build",
    "build:full:compile": "concurrently npm:build:compile",
    "alfred": "node dist/alfred/www.js",
    "alfred:debug": "node --inspect=0.0.0.0:5858 dist/alfred/www.js",
    "deli": "node dist/deli/index.js",
    "deli:debug": "node --inspect=0.0.0.0:5858 dist/deli/index.js",
    "docker-run": "docker run --rm -v $(pwd):/usr/src/server -w /usr/src/server node:8.14.0-slim",
    "paparazzi": "node dist/paparazzi/index.js",
    "paparazzi:debug": "node --inspect=0.0.0.0:5858 dist/paparazzi/index.js",
    "scriptorium": "node dist/scriptorium/index.js",
    "scriptorium:debug": "node --inspect=0.0.0.0:5858 dist/scriptorium/index.js",
    "foreman": "node dist/foreman/index.js",
    "foreman:debug": "node --inspect=0.0.0.0:5858 dist/foreman/index.js",
    "test": "mocha --recursive dist/test",
    "test:coverage": "nyc npm test -- --reporter mocha-junit-reporter --reporter-options mochaFile=nyc/junit-report.xml",
    "tsc": "tsc",
    "tslint": "tslint --project tsconfig.json --format verbose",
    "beast": "npm test -- --beast --grep beastTest"
  },
  "author": "Microsoft",
  "repository": "microsoft/prague",
  "browser": {
    "request": "xhr"
  },
  "nyc": {
    "all": true,
    "exclude": [
      "src/test/**/*.ts",
      "dist/test/**/*.js"
    ],
    "include": [
      "src/**/*.ts",
      "dist/**/*.js"
    ],
    "reporter": [
      "cobertura",
      "html",
      "text"
    ],
    "temp-directory": "nyc/.nyc_output",
    "cache-dir": "nyc/.cache",
    "report-dir": "nyc/report"
  },
  "dependencies": {
    "@prague/agent": "^0.9.0",
    "@prague/client-api": "^0.9.0",
    "@prague/component-core-interfaces": "^0.9.0",
    "@prague/gitresources": "^0.9.0",
    "@prague/kafka-orderer": "^0.9.0",
    "@prague/lambdas": "^0.9.0",
    "@prague/lambdas-driver": "^0.9.0",
    "@prague/memory-orderer": "^0.9.0",
    "@prague/merge-tree": "^0.9.0",
    "@prague/protocol-definitions": "^0.9.0",
    "@prague/routerlicious-host": "^0.9.0",
    "@prague/routerlicious-socket-storage": "^0.9.0",
    "@prague/runtime-definitions": "^0.9.0",
    "@prague/sequence": "^0.9.0",
    "@prague/services": "^0.9.0",
    "@prague/services-client": "^0.9.0",
    "@prague/services-core": "^0.9.0",
    "@prague/services-utils": "^0.9.0",
    "@prague/socket-storage-shared": "^0.9.0",
    "@prague/tools-core": "^0.9.0",
    "@prague/utils": "^0.9.0",
    "@types/semver": "^6.0.0",
    "body-parser": "^1.17.1",
    "bytes": "^3.0.0",
    "commander": "^2.17.1",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "hjs": "^0.0.6",
    "json-stringify-safe": "^5.0.1",
    "jsonwebtoken": "^8.4.0",
    "lodash": "^4.17.11",
    "minio": "^7.0.2",
    "moniker": "^0.1.2",
    "morgan": "^1.8.1",
    "nconf": "^0.10.0",
    "progress": "^2.0.3",
    "redis": "^2.8.0",
    "request": "^2.88.0",
    "semver": "^6.0.0",
    "split": "^1.0.0",
    "unzip-stream": "^0.3.0",
    "webpack": "^4.8.2",
    "winston": "^3.1.0",
    "ws": "^6.1.2"
  },
  "devDependencies": {
    "@prague/build-common": "^0.9.0",
    "@prague/test-utils": "^0.9.0",
    "@types/bytes": "^3.0.0",
    "@types/compression": "0.0.33",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.4",
    "@types/diff": "^3.5.1",
    "@types/json-stringify-safe": "^5.0.0",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/lodash": "^4.14.118",
    "@types/mocha": "^5.2.5",
    "@types/morgan": "^1.7.35",
    "@types/nconf": "^0.0.37",
    "@types/node": "^10.12.12",
    "@types/progress": "^2.0.3",
    "@types/random-js": "^1.0.31",
    "@types/redis": "^2.8.8",
    "@types/request": "^2.47.1",
    "@types/split": "^0.3.28",
    "@types/supertest": "^2.0.5",
    "@types/ws": "^6.0.1",
    "cache-loader": "^1.2.2",
    "concurrently": "^4.1.0",
    "css-loader": "^1.0.0",
    "diff": "^3.5.0",
    "html-loader": "^0.5.5",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.18.0",
    "node-sass": "^4.9.3",
    "nyc": "^14.1.1",
    "random-js": "^1.0.8",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "source-map-loader": "^0.2.4",
    "string-hash": "^1.1.3",
    "style-loader": "^0.23.0",
    "supertest": "^3.1.0",
    "thread-loader": "^1.2.0",
    "ts-loader": "^4.5.0",
    "tslint": "^5.11.0",
    "typescript": "~3.4.5",
    "url-loader": "^1.1.1",
    "ws": "^6.1.2"
  },
  "license": "MIT"
}
