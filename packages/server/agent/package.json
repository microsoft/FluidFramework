    "@fluid-internal/client-api": "^0.12.0",
    "@microsoft/fluid-component-core-interfaces": "^0.12.0",
    "@microsoft/fluid-container-definitions": "^0.12.0",
    "@microsoft/fluid-container-loader": "^0.12.0",
    "@microsoft/fluid-core-utils": "^0.12.0",
    "@microsoft/fluid-driver-definitions": "^0.12.0",
    "@microsoft/fluid-map": "^0.12.0",
    "@microsoft/fluid-merge-tree": "^0.12.0",
    "@microsoft/fluid-protocol-definitions": "^0.12.0",
    "@microsoft/fluid-sequence": "^0.12.0",
    "azure-iot-device": "^1.7.3",
    "azure-iot-device-mqtt": "^1.7.3",
    "azure-iothub": "^1.7.3",
    "debug": "^4.1.1",
    "nconf": "^0.10.0",
    "request": "^2.88.0",
    "telegrafjs": "^0.1.3"
  },
  "devDependencies": {
    "@microsoft/fluid-build-common": "^0.12.0",
    "@types/debug": "^0.0.31",
    "@types/nconf": "^0.0.37",
    "@types/node": "^10.12.12",
    "@types/request": "^2.47.1",
    "concurrently": "^4.1.0",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "typescript": "~3.4.5"
  }
}
