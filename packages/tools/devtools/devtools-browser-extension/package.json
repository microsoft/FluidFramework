{
	"name": "@fluid-internal/devtools-browser-extension",
	"version": "2.1.0",
	"private": true,
	"description": "A browser extension for visualizing Fluid Framework stats and operations",
	"homepage": "https://fluidframework.com",
	"repository": {
		"type": "git",
		"url": "https://github.com/microsoft/FluidFramework.git",
		"directory": "packages/tools/devtools/devtools-browser-extension"
	},
	"license": "MIT",
	"author": "Microsoft and contributors",
	"type": "module",
	"scripts": {
		"build": "fluid-build . --task build",
		"build-and-test": "npm run build && npm run test",
		"build:compile": "fluid-build . --task compile",
		"build:esnext": "tsc --project ./tsconfig.json",
		"build:test": "npm run build:test:esm && npm run build:test:cjs",
		"build:test:cjs": "fluid-tsc commonjs --project ./e2e-tests/tsconfig.cjs.json",
		"build:test:esm": "tsc --project ./e2e-tests/tsconfig.json",
		"build:webpack": "npm run webpack",
		"check:biome": "biome check . --formatter-enabled=true",
		"check:format": "npm run check:biome",
		"check:prettier": "prettier --check . --cache --ignore-path ../../../../.prettierignore",
		"clean": "rimraf --glob coverage dist lib nyc \"**/*.tsbuildinfo\" \"**/*.build.log\"",
		"eslint": "eslint src",
		"eslint:fix": "eslint src --fix",
		"format": "npm run format:biome",
		"format-and-build": "npm run format && npm run build",
		"format-and-compile": "npm run format && npm run build:compile",
		"format:biome": "biome check . --formatter-enabled=true --apply",
		"format:prettier": "prettier --write . --cache --ignore-path ../../../../.prettierignore",
		"good-fences": "gf",
		"lint": "fluid-build . --task lint",
		"lint:fix": "fluid-build . --task eslint:fix --task format",
		"rebuild": "npm run clean && npm run build",
		"start:client:test": "webpack serve --config webpack.test.cjs",
		"test": "concurrently npm:test:mocha npm:test:jest",
		"test:coverage": "c8 npm test",
		"test:end-to-end": "npm run test:jest",
		"test:jest": "jest",
		"test:jest:verbose": "cross-env FLUID_TEST_VERBOSE=1 jest",
		"test:mocha": "npm run test:mocha:esm && npm run test:mocha:cjs",
		"test:mocha:cjs": "mocha --recursive \"dist/tsc/test/**/*.test.*js\" --exit",
		"test:mocha:esm": "echo \"Skipping ESM tests -- See AB#7470\"",
		"test:mocha:verbose": "cross-env FLUID_TEST_VERBOSE=1 npm run mocha -- --verbose",
		"test:unit": "npm run test:mocha",
		"test:verbose": "concurrently npm:test:mocha:verbose npm:test:jest:verbose",
		"tsc": "fluid-tsc commonjs --project ./tsconfig.cjs.json && copyfiles -f ../../../../common/build/build-common/src/cjs/package.json ./dist",
		"webpack": "webpack",
		"webpack:dev": "webpack --env development"
	},
	"c8": {
		"all": true,
		"cache-dir": "nyc/.cache",
		"exclude": [
			"src/test/**/*.*ts",
			"dist/tsc/test/**/*.*js",
			"dist/e2e-tests/**",
			"dist/bundle/**"
		],
		"exclude-after-remap": false,
		"include": [
			"src/**/*.*ts",
			"dist/tsc/**/*.*js"
		],
		"report-dir": "nyc/report",
		"reporter": [
			"cobertura",
			"html",
			"text"
		],
		"temp-directory": "nyc/.nyc_output"
	},
	"dependencies": {
		"@fluid-internal/client-utils": "workspace:~",
		"@fluid-internal/devtools-view": "workspace:~",
		"@fluidframework/core-interfaces": "workspace:~",
		"@fluidframework/core-utils": "workspace:~",
		"@fluidframework/devtools-core": "workspace:~",
		"@microsoft/1ds-core-js": "^3.2.13",
		"@microsoft/1ds-post-js": "^3.2.13",
		"@types/uuid": "^9.0.2",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"tslib": "^1.10.0",
		"uuid": "^9.0.0"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.7.3",
		"@fluid-example/example-utils": "workspace:~",
		"@fluid-internal/mocha-test-setup": "workspace:~",
		"@fluidframework/aqueduct": "workspace:~",
		"@fluidframework/build-common": "^2.0.3",
		"@fluidframework/build-tools": "^0.39.0",
		"@fluidframework/container-definitions": "workspace:~",
		"@fluidframework/container-loader": "workspace:~",
		"@fluidframework/container-runtime-definitions": "workspace:~",
		"@fluidframework/eslint-config-fluid": "^5.3.0",
		"@fluidframework/runtime-utils": "workspace:~",
		"@fluidframework/sequence": "workspace:~",
		"@fluidframework/test-utils": "workspace:~",
		"@types/chai": "^4.0.0",
		"@types/chrome": "0.0.232",
		"@types/jest": "29.5.3",
		"@types/jest-environment-puppeteer": "workspace:~",
		"@types/jsdom": "^21.1.1",
		"@types/jsdom-global": "^3.0.4",
		"@types/mocha": "^9.1.1",
		"@types/node": "^18.19.0",
		"@types/proxyquire": "^1.3.28",
		"@types/react": "^17.0.44",
		"@types/react-dom": "^17.0.18",
		"@types/sinon": "^17.0.3",
		"@types/sinon-chrome": "^2.2.11",
		"c8": "^8.0.1",
		"chai": "^4.2.0",
		"concurrently": "^8.2.1",
		"copy-webpack-plugin": "^11.0.0",
		"copyfiles": "^2.4.1",
		"cross-env": "^7.0.3",
		"dotenv-webpack": "^7.0.3",
		"eslint": "~8.55.0",
		"eslint-config-prettier": "~9.0.0",
		"eslint-plugin-chai-expect": "~3.0.0",
		"expect-puppeteer": "^9.0.2",
		"good-fences": "^1.1.1",
		"html-webpack-plugin": "^5.5.0",
		"jest": "^29.6.2",
		"jest-dev-server": "^9.0.2",
		"jest-environment-puppeteer": "^9.0.2",
		"jest-junit": "^10.0.0",
		"jest-puppeteer": "^9.0.2",
		"jsdom": "^16.7.0",
		"jsdom-global": "^3.0.2",
		"mocha": "^10.2.0",
		"prettier": "~3.0.3",
		"proxyquire": "^2.1.3",
		"puppeteer": "^22.2.0",
		"rimraf": "^4.4.0",
		"sinon": "^17.0.1",
		"sinon-chrome": "^3.0.1",
		"ts-jest": "^29.1.1",
		"ts-loader": "^9.3.0",
		"typescript": "~5.4.5",
		"webpack": "^5.82.0",
		"webpack-cli": "^4.9.2"
	},
	"fluidBuild": {
		"tasks": {
			"compile": {
				"dependsOn": [
					"...",
					"webpack"
				],
				"script": false
			}
		}
	},
	"typeValidation": {
		"disabled": true,
		"broken": {}
	}
}
