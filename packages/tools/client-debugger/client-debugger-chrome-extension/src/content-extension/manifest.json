{
	"manifest_version": 3,
	"name": "Fluid Client Debugger: Content",
	"description": "Displays a live debug view of the Fluid client for your application.",
	"version": "1.0",
	"permissions": ["activeTab", "scripting", "storage"],
	"action": {
		"default_icon": "images/icon.png"
	},
	"background": {
		"service_worker": "BackgroundScript.js"
	},
	"content_scripts": [
		{
			"js": ["ContentScript.js"],
			"matches": ["<all_urls>"]
		}
	]
}
