{
    "name": "@fluid-tools/client-debugger-chrome-extension",
    "version": "0.1.0",
    "private": true,
    "description": "A Chrome extension visual debugger for the Fluid Client.",
    "homepage": "https://fluidframework.com",
    "repository": {
        "type": "git",
        "url": "https://github.com/microsoft/FluidFramework.git",
        "directory": "packages/tools/client-debugger/client-debugger-chrome-extension"
    },
    "license": "MIT",
    "author": "Microsoft and contributors",
    "scripts": {
        "build": "npm run build:genver && concurrently npm:build:compile npm:lint",
        "build:compile": "npm run build:tsc && npm run webpack",
        "build:full": "npm run build",
        "build:full:compile": "npm run build:compile",
        "build:genver": "gen-version",
        "build:tsc": "npm run tsc",
        "build:webpack": "npm run webpack",
        "clean": "rimraf coverage dist nyc *.tsbuildinfo *.build.log",
        "eslint": "eslint src",
        "eslint:fix": "eslint src --fix",
        "format": "npm run prettier:fix",
        "format-and-build": "npm run format && npm run build",
        "format-and-compile": "npm run format && npm run build:compile",
        "lint": "npm run prettier && npm run eslint",
        "lint:fix": "npm run prettier:fix && npm run eslint:fix",
        "mocha": "mocha dist/**/*.test.js --unhandled-rejections=strict --exit",
        "prettier": "prettier --check .",
        "prettier:fix": "prettier --write .",
        "start:tinylicious": "tinylicious",
        "test": "start-server-and-test start:tinylicious 7070 test:jest",
        "test:coverage": "start-server-and-test start:tinylicious 7070 test:jest:coverage",
        "test:jest": "jest --detectOpenHandles",
        "test:jest:coverage": "jest  --detectOpenHandles --coverage --ci --reporters=default --reporters=jest-junit",
        "tsc": "tsc --pretty",
        "webpack": "webpack --env production",
        "webpack:dev": "webpack --env development"
    },
    "dependencies": {
        "@fluid-tools/client-debugger": ">=2.0.0-internal.1.2.0 <2.0.0-internal.3.0.0",
        "@fluid-tools/client-debugger-view": ">=2.0.0-internal.1.2.0 <2.0.0-internal.3.0.0",
        "@fluidframework/common-utils": "^1.0.0",
        "@fluidframework/core-interfaces": ">=2.0.0-internal.1.2.0 <2.0.0-internal.3.0.0",
        "@fluidframework/counter": ">=2.0.0-internal.1.2.0 <2.0.0-internal.3.0.0",
        "@fluidframework/driver-definitions": ">=2.0.0-internal.1.2.0 <2.0.0-internal.3.0.0",
        "@fluidframework/protocol-definitions": "^1.1.0",
        "react": "^17.0.1",
        "react-dom": "^17.0.1"
    },
    "devDependencies": {
        "@fluentui/react": "^8.96.1",
        "@fluidframework/build-common": "^1.1.0",
        "@fluidframework/build-tools": "^0.7.0",
        "@fluidframework/container-loader": ">=2.0.0-internal.2.1.0 <2.0.0-internal.3.0.0",
        "@fluidframework/eslint-config-fluid": "^2.0.0",
        "@fluidframework/fluid-static": ">=2.0.0-internal.1.2.0 <2.0.0-internal.3.0.0",
        "@fluidframework/test-utils": "^1.3.0",
        "@fluidframework/tinylicious-client": ">=2.0.0-internal.1.2.0 <2.0.0-internal.3.0.0",
        "@microsoft/api-extractor": "^7.22.2",
        "@trivago/prettier-plugin-sort-imports": "^3.3.0",
        "@types/chrome": "0.0.200",
        "@types/jest": "22.2.3",
        "concurrently": "^6.2.0",
        "copy-webpack-plugin": "^11.0.0",
        "copyfiles": "^2.4.1",
        "eslint": "~8.6.0",
        "eslint-config-prettier": "~8.5.0",
        "eslint-plugin-jest": "^27.1.5",
        "html-webpack-plugin": "^5.5.0",
        "jest": "^26.6.3",
        "jest-junit": "^10.0.0",
        "jest-webextension-mock": "^3.7.22",
        "prettier": "~2.6.2",
        "rimraf": "^2.6.2",
        "start-server-and-test": "^1.11.7",
        "tinylicious": "0.4.89251",
        "ts-jest": "^26.4.4",
        "ts-loader": "^9.3.0",
        "typescript": "~4.5.5",
        "webpack": "^5.72.0",
        "webpack-cli": "^4.9.2"
    },
    "jest-junit": {
        "outputDirectory": "nyc",
        "outputName": "jest-junit-report.xml"
    }
}
