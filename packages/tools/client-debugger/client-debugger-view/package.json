{
	"name": "@fluid-tools/client-debugger-view",
	"version": "2.0.0-internal.4.1.0",
	"private": true,
	"description": "Contains an extensible debug visualizer / editor for the Fluid client",
	"homepage": "https://fluidframework.com",
	"repository": {
		"type": "git",
		"url": "https://github.com/microsoft/FluidFramework.git",
		"directory": "packages/tools/client-debugger/client-debugger-view"
	},
	"license": "MIT",
	"author": "Microsoft and contributors",
	"sideEffects": false,
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"build": "npm run build:genver && concurrently npm:build:compile npm:lint && npm run build:docs",
		"build:compile": "npm run build:tsc && npm run build:copy-resources",
		"build:copy-resources": "copyfiles \"src/**/*.css\" dist --up 1",
		"build:docs": "api-extractor run --local && copyfiles -u 1 ./_api-extractor-temp/doc-models/* ../../../../_api-extractor-temp/",
		"build:full": "concurrently npm:build npm:webpack",
		"build:full:compile": "concurrently npm:build:compile npm:webpack",
		"build:genver": "gen-version",
		"build:tsc": "npm run tsc",
		"ci:build:docs": "api-extractor run && copyfiles -u 1 ./_api-extractor-temp/doc-models/* ../../../../_api-extractor-temp/",
		"clean": "rimraf _api-extractor-temp coverage dist nyc *.tsbuildinfo *.build.log",
		"eslint": "eslint src",
		"eslint:fix": "eslint src --fix",
		"format": "npm run prettier:fix",
		"format-and-build": "npm run format && npm run build",
		"format-and-compile": "npm run format && npm run build:compile",
		"lint": "npm run prettier && npm run eslint",
		"lint:fix": "npm run prettier:fix && npm run eslint:fix",
		"prettier": "prettier --check . --ignore-path ../../../../.prettierignore",
		"prettier:fix": "prettier --write . --ignore-path ../../../../.prettierignore",
		"start:test-app": "start-server-and-test start:tinylicious 7070 start:test-app:client",
		"start:test-app:client": "webpack serve --config webpack.config.js",
		"start:tinylicious": "tinylicious",
		"test": "npm run test:jest",
		"test:coverage": "npm run test:jest:coverage",
		"test:jest": "jest --detectOpenHandles",
		"test:jest:coverage": "jest --detectOpenHandles --coverage --ci --reporters=default --reporters=jest-junite",
		"tsc": "tsc",
		"webpack": "webpack --env production",
		"webpack:dev": "webpack --env development"
	},
	"dependencies": {
		"@fluentui/react": "^8.96.1",
		"@fluentui/react-components": "^9.18.6",
		"@fluentui/react-hooks": "^8.6.12",
		"@fluentui/react-icons": "^2.0.199",
		"@fluentui/react-icons-mdl2": "^1.3.22",
		"@fluentui/react-tabs": "^9.3.6",
		"@fluid-experimental/react-inputs": ">=2.0.0-internal.4.1.0 <2.0.0-internal.4.2.0",
		"@fluid-tools/client-debugger": ">=2.0.0-internal.4.1.0 <2.0.0-internal.4.2.0",
		"@fluidframework/common-definitions": "^0.20.1",
		"@fluidframework/common-utils": "^1.1.1",
		"@fluidframework/container-definitions": ">=2.0.0-internal.4.1.0 <2.0.0-internal.4.2.0",
		"@fluidframework/container-loader": ">=2.0.0-internal.4.1.0 <2.0.0-internal.4.2.0",
		"@fluidframework/core-interfaces": ">=2.0.0-internal.4.1.0 <2.0.0-internal.4.2.0",
		"@fluidframework/counter": ">=2.0.0-internal.4.1.0 <2.0.0-internal.4.2.0",
		"@fluidframework/datastore": ">=2.0.0-internal.4.1.0 <2.0.0-internal.4.2.0",
		"@fluidframework/driver-definitions": ">=2.0.0-internal.4.1.0 <2.0.0-internal.4.2.0",
		"@fluidframework/map": ">=2.0.0-internal.4.1.0 <2.0.0-internal.4.2.0",
		"@fluidframework/protocol-definitions": "^1.1.0",
		"@fluidframework/sequence": ">=2.0.0-internal.4.1.0 <2.0.0-internal.4.2.0",
		"@fluidframework/shared-object-base": ">=2.0.0-internal.4.1.0 <2.0.0-internal.4.2.0",
		"re-resizable": "^6.9.9",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-json-view": "^1.21.3",
		"scheduler": "^0.20.0"
	},
	"devDependencies": {
		"@fluid-example/collaborative-textarea": ">=2.0.0-internal.4.1.0 <2.0.0-internal.4.2.0",
		"@fluid-tools/webpack-fluid-loader": ">=2.0.0-internal.4.1.0 <2.0.0-internal.4.2.0",
		"@fluidframework/build-common": "^1.1.0",
		"@fluidframework/build-tools": "^0.13.1",
		"@fluidframework/eslint-config-fluid": "^2.0.0",
		"@fluidframework/fluid-static": ">=2.0.0-internal.4.1.0 <2.0.0-internal.4.2.0",
		"@fluidframework/test-runtime-utils": ">=2.0.0-internal.4.1.0 <2.0.0-internal.4.2.0",
		"@fluidframework/tinylicious-client": ">=2.0.0-internal.4.1.0 <2.0.0-internal.4.2.0",
		"@microsoft/api-extractor": "^7.34.4",
		"@testing-library/dom": "^8.2.0",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^12.0.0",
		"@testing-library/user-event": "^14.4.3",
		"@types/jest": "22.2.3",
		"@types/react": "^17.0.44",
		"@types/react-dom": "^17.0.18",
		"@types/testing-library__jest-dom": "^5.14.5",
		"concurrently": "^7.6.0",
		"copyfiles": "^2.4.1",
		"cross-env": "^7.0.3",
		"eslint": "~8.6.0",
		"eslint-config-prettier": "~8.5.0",
		"eslint-plugin-jest": "~27.1.5",
		"eslint-plugin-react": "~7.28.0",
		"eslint-plugin-react-hooks": "~4.6.0",
		"jest": "^26.6.3",
		"jest-junit": "^10.0.0",
		"prettier": "~2.6.2",
		"rimraf": "^4.4.0",
		"start-server-and-test": "^1.11.7",
		"style-loader": "^1.0.0",
		"tinylicious": "0.7.2",
		"ts-jest": "^26.4.4",
		"ts-loader": "^9.3.0",
		"typescript": "~4.5.5",
		"webpack": "^5.76.2",
		"webpack-cli": "^4.9.2",
		"webpack-dev-server": "~4.6.0",
		"webpack-merge": "^5.8.0"
	},
	"fluid": {
		"browser": {
			"umd": {
				"files": [
					"dist/main.bundle.js"
				],
				"library": "main"
			}
		}
	},
	"jest-junit": {
		"outputDirectory": "nyc",
		"outputName": "jest-junit-report.xml"
	},
	"typeValidation": {
		"disabled": true,
		"broken": {}
	}
}
